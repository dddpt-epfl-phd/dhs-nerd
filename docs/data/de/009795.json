ll, "extra_fields": {"rawName": "Germanisten", "confidence_score": 0.4119, "domains": ["Biology", "Sociology"], "origin": "entity_fishing"}}}, {"start": 850, "end": 869, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q120789", "wikipedia_page_id": 4856185, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q120789", "grobid_tag": null, "extra_fields": {"rawName": "Germanisten", "confidence_score": 0.4119, "domains": ["Biology", "Sociology"], "origin": "entity_fishing"}}, "href": "de/articles/012310", "dhsid": "012310", "wiki": {"item": "http://www.wikidata.org/entity/Q120789", "itemLabel": "Franz Joseph Stalder", "dhsid": "012310", "namefr": "", "articlefr": "", "namede": "Franz Joseph Stalder", "articlede": "https://de.wikipedia.org/wiki/Franz_Joseph_Stalder", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "11720613X", "wikidata_id": "Q120789"}}, {"start": 908, "end": 919, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q166153", "wikipedia_page_id": 31851, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q166153", "grobid_tag": null, "extra_fields": {"rawName": "Germanisten", "confidence_score": 0.4119, "domains": ["Biology", "Sociology"], "origin": "entity_fishing"}}}]], "sources": {"Literatur": [{"text": "E. Studer, Leonz F., 1952", "author": ["E. Studer"], "tpub": ["Leonz F., 1952"]}, {"text": "HS II/2, 357", "tpub": ["HS II/2, 357"]}]}, "metagrid_id": "9795", "metagrid_links": [{}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/118703412"}, {"title": "VIAF", "url": "http://viaf.org/viaf/54942920"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 20.4.1768\n   \u271d\ufe0e 21.3.1840\n1768-04-201840-03-21", "birth_date": "1768-04-20", "death_date": "1840-03-21"}], "birth_date": "1768-04-20", "death_date": "1840-03-21", "text_blocks": [["h1", "Leonz F\u00fcglistaller"], ["p", "20.4.1768 Jonen, 21.3.1840 Luzern, kath., von Lieli (heute Gem. Oberwil-Lieli). Sohn des Josef, M\u00fcllers. Stud. in Solothurn und 1791-93 in Dillingen (Bayern) als Sch\u00fcler Johann Michael Sailers, 1792 Priester. 1793-97 Vikar in Wolhusen und Rothenburg, dann Lateinlehrer in Rapperswil (SG; 1797) und Luzern (1798), hier seit 1801 Prof. f\u00fcr Mathematik und Physik am Gymnasium, 1807 f\u00fcr Moraltheologie an der Theol. Fakult\u00e4t und Referent f\u00fcr das kant. Landschulwesen. 1810-11 Stud. der Physik in G\u00f6ttingen und Paris, danach 1811-15 Prof. am kath. Gymnasium in St. Gallen und Adjunkt der Stiftsbibliothek. 1815-17 Kanzler des apostol. Vikars Franz Bernhard G\u00f6ldlin von Tiefenau, 1818-19 Lehrer in Zurzach, 1819-1824 wieder in Luzern. 1824 Chorherr, 1832 Propst des Stifts St. Leodegar. Der vielseitig gebildete Sprachforscher arbeitete am \"Idiotikon\" von Franz Josef Stalder mit und korrespondierte mit f\u00fchrenden Germanisten seiner Zeit. Seine wissenschaftl. Arbeiten blieben indes weithin ungedruckt."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q94789044", "itemLabel": "Q94789044", "dhsid": "009795", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1037751825"}, {"item": "http://www.wikidata.org/entity/Q94789044", "itemLabel": "Q94789044", "dhsid": "009795", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "118703412"}], "wikidata_url": "http://www.wikidata.org/entity/Q94789044", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://h