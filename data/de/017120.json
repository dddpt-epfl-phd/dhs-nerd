ur": [{"text": "Slg. Bern. Biogr. 1, 1884, 387-393", "tpub": ["Slg. Bern. Biogr. 1, 1884, 387-393"]}, {"text": "W. Maync, Bern. Wohnschl\u00f6sser, 1979", "author": ["W. Maync"], "tpub": ["Bern. Wohnschl\u00f6sser, 1979"]}]}, "metagrid_id": "17120", "metagrid_links": [{}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/13791671X"}], "bref": [{"title": "Lebensdaten", "text": "\u2248\ufe0e 14.8.1634\n   \u271d\ufe0e 14.10.1710\n1710-10-14", "death_date": "1710-10-14"}], "death_date": "1710-10-14", "text_blocks": [["h1", "Johann Bernhard von Muralt"], ["p", "14.8.1634 Bern, 14.10.1710 Bern, ref., von Bern. Sohn des Jost, Grossrats und Landvogts, und der Marie Schweizereisen. Bruder des Franz Ludwig (->). 1661 Johanna Katharina Steiger (weiss), Tochter des Georg, bern. Kleinrats. 1664 Grossrat in Bern, 1671 Grossweibel, 1674-80 Hofmeister in K\u00f6nigsfelden, 1684 Kleinrat in Bern, 1693-99 Deutschs\u00e4ckelmeister, 1709 Venner zu Gerbern. M. kaufte 1694 von Daniel St\u00fcrler die Freiherrschaft und das Schloss Belp. Er war oft bern. Gesandter: 1686 nach Savoyen der ref. Waldenser wegen, 1690-1709 mehrmals an die Tagsatzung in Luzern und Z\u00fcrich, 1703-04 nach Savoyen und Frankreich, 1705 ins M\u00fcnstertal wegen \u00dcbergriffen des Bf. von Basel. 1698 sprach er sich gegen die harte Behandlung der bern. Pietisten aus."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q55857445", "itemLabel": "Johann Bernhard von Muralt", "dhsid": "017120", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "13791671X"}], "wikidata_url": "http://www.wikidata.org/entity/Q55857445", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "M", "url": "https://hls-dhs-dss.ch/de/articles/017120/2009-01-23"}