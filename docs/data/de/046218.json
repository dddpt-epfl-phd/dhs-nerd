rid_id": "46218", "metagrid_links": [{"AfA": {"url": "https://www.histoirerurale.ch/pers/personnes/5548.html", "short_description": "Archiv f\u00fcr Agrargeschichte", "long_description": "Archiv f\u00fcr Agrargeschichte"}, "Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=92916", "short_description": "Schweizerische Eliten im 20. Jahrhundert", "long_description": "Schweizerische Eliten im 20. Jahrhundert"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/1059897407"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 23.4.1844\n   \u271d\ufe0e 18.8.1913\n1844-04-231913-08-18", "birth_date": "1844-04-23", "death_date": "1913-08-18"}], "birth_date": "1844-04-23", "death_date": "1913-08-18", "text_blocks": [["h1", "Arnold Rossel"], ["p", "23.4.1844 Courtelary, 18.8.1913 Saignel\u00e9gier, ref., von Pr\u00eales. Sohn des Aim\u00e9 (->). \u200b1) 1874 Maria Amalia Carolina Weller, Tochter des Fran\u00e7ois, 2) 1884 Pauline Gut geb. Zehnder, Witwe des Jean. Stud. der Chemie am Eidg. Polytechnikum in Z\u00fcrich (1864-67), in Leipzig und Stuttgart. 1868 Promotion an der Univ. Z\u00fcrich. Ab 1874 Chef des Labors f\u00fcr Agrarchemie an der Landwirtschaftsschule R\u00fctti (Gem. Zollikofen), ab 1875 Lehrer am Technikum Winterthur, 1890-96 o. Prof. f\u00fcr anorgan. Chemie an der Univ. Bern (1892-93 Dekan). Ab 1896 Direktor der Karbidfabrik Attisholz und Inspektor der Karbidfabriken der Schweiz. 1871-72 und 1903-13 freisinniger Berner Grossrat. R. erwarb sich Verdienste um die popularisierende Vermittlung wissenschaftl. Erkenntnisse, die n\u00fctzl. Anwendung chem. Entdeckungen in Landwirtschaft und Rebbau sowie deren industrielle Ausbeutung. R. war Initiant der Standseilbahn Ligerz-Pr\u00eales und beteiligte sich an der Urbarisierung des Gr. Mooses."]], "wiki_links": [], "wikidata_url": null, "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "R", "url": "https:/