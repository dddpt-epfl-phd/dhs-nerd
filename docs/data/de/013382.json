otice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/118723804"}, {"title": "VIAF", "url": "http://viaf.org/viaf/49245352"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 9.12.1909\n   \u271d\ufe0e 14.12.2002\n1909-12-092002-12-14", "birth_date": "1909-12-09", "death_date": "2002-12-14"}], "birth_date": "1909-12-09", "death_date": "2002-12-14", "text_blocks": [["h1", "Albert Knoepfli"], ["p", "9.12.1909 Bischofszell, 14.12.2002 Aadorf, ref., von Illighausen (Gem. Lengwil), ab 1982 Ehrenb\u00fcrger von Bischofszell, ab 1989 Ehrenb\u00fcrger von Aadorf. Sohn des Heinrich Albert, Bankbeamten, und der Ernestine geb. M\u00fcller. 1939 Eva Schaufelberger. Lehrerseminar Kreuzlingen. Stud. der Kunst- und Musikwissenschaften, Geschichte, Germanistik und Romanistik in Basel, Grenoble und Perugia. 1935-45 Sekundarlehrer in Aadorf. 1945-74 Inventarisator der thurg. Kunstdenkm\u00e4ler und erster thurg. Denkmalpfleger. Ab 1964 Dozent (1972 Titularprof.) f\u00fcr Denkmalpflege an der ETH Z\u00fcrich, 1972-79 Leiter des von ihm gegr. ETH-Instituts f\u00fcr Denkmalpflege. Zahlreiche Ehrungen, u.a. 1962 Dr. h.c. der Univ. Z\u00fcrich, 1982 Schinkelring des Dt. Nationalkomitees f\u00fcr Denkmalschutz."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q1450522", "itemLabel": "Albert Knoepfli", "dhsid": "013382", "namefr": "", "articlefr": "", "namede": "Albert Knoepfli", "articlede": "https://de.wikipedia.org/wiki/Albert_Knoepfli", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "118723804"}], "wikidata_url": "http://www.wikidata.org/entity/Q1450522", "wikipedia_page_title": "Albert Knoepfli", "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/de/articles/013382/2007-08-21"}