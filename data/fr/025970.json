0 1962."]], "text_links": [[{"start": 0, "end": 14, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "PERSON", "extra_fields": {"rawName": "Soleure", "confidence_score": 0.5128, "domains": ["Administration", "Nautical", "Transport", "Environment", "Gas", "Sociology", "Geology"], "origin": "entity_fishing"}}}], [{"start": 12, "end": 19, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q68965", "wikipedia_page_id": 182045, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q68965", "grobid_tag": null, "extra_fields": {"rawName": "Soleure", "confidence_score": 0.5128, "domains": ["Administration", "Nautical", "Transport", "Environment", "Gas", "Sociology", "Geology"], "origin": "entity_fishing"}}, "href": "fr/articles/001174", "dhsid": "001174", "wiki": {"item": "http://www.wikidata.org/entity/Q68965", "itemLabel": "Soleure", "dhsid": "001174", "namefr": "Soleure", "articlefr": "https://fr.wikipedia.org/wiki/Soleure", "namede": "Solothurn", "articlede": "https://de.wikipedia.org/wiki/Solothurn", "nameit": "Soletta (Svizzera)", "articleit": "https://it.wikipedia.org/wiki/Soletta_(Svizzera)", "nameen": "Solothurn", "articleen": "https://en.wikipedia.org/wiki/Solothurn", "instanceof": "http://www.wikidata.org/entity/Q54935504", "instanceofLabel": "ville de Suisse", "gndid": "4055458-2", "wikidata_id": "Q68965"}}, {"start": 33, "end": 38, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q70", "wikipedia_page_id": 32050, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q70", "grobid_tag": null, "extra_fields": {"rawName": "Soleure", "confidence_score": 0.5128, "domains": ["Administration", "Nautical", "Transport", "Environment", "Gas", "Sociology", "Geology"], "origin": "entity_fishing"}}, "href": "fr/