erung."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q1458592", "itemLabel": "Friedrich Buri", "dhsid": "004450", "namefr": "", "articlefr": "", "namede": "Friedrich Buri", "articlede": "https://de.wikipedia.org/wiki/Friedrich_Buri", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1052039332"}], "wikidata_url": "http://www.wikidata.org/entity/Q1458592", "wikipedia_page_title": "Friedrich Buri", "added_wikidata_wikipedia_to_text_links": true, "_initial": "B", "url": "https://hls-dhs-dss.ch/de/articles/004450/2003-04-15"}