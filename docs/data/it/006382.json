tagrid_id": "6382", "metagrid_links": [{"Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=56287", "short_description": "Elites suisses au XXe si\u00e8cle", "long_description": "Elites suisses au XXe si\u00e8cle"}, "parl.ch": {"url": "https://www.parlament.ch/de/biografie?CouncillorId=2341", "short_description": "Members of the Swiss parliament", "long_description": "Members of the Swiss National Council and the Council of States since 1848"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/1059409771"}, {"title": "VIAF", "url": "http://viaf.org/viaf/311187694"}], "bref": [{"title": "Dati biografici", "text": "\u2217\ufe0e 28.4.1869\n   \u271d\ufe0e 22.9.1955\n1869-04-281955-09-22", "birth_date": "1869-04-28", "death_date": "1955-09-22"}], "birth_date": "1869-04-28", "death_date": "1955-09-22", "text_blocks": [["h1", "Alfred Hofstetter"], ["p", "28.4.1869 Gais, 22.9.1955 Gais, rif., di Gais. Figlio di Johannes (1832-1908), socio di un'azienda attiva nel commercio di filati, membro del Gran Consiglio e del Consiglio di Stato, e di Katharina Koller. \u200b1) (1896) Frida Aerne, figlia di Johann Ulrich, fabbricante di tessili, giudice del tribunale d'appello e membro del Gran Consiglio per Urn\u00e4sch; 2) (1922) Maria Freund, figlia di Ulrich, agricoltore. Dopo la scuola cant. a Trogen e la maturit\u00e0 a Zurigo (1888), studi\u00f2 diritto a Zurigo, Berlino, Losanna e Berna, conseguendo il dottorato nel 1894. Nello stesso anno apr\u00ec uno studio legale a Gais e fu consulente giur. nel settore bancario. Municipale di Gais (1894-98), fu sindaco (1901-07), membro del Gran Consiglio (1900-10 e 1921-34, pi\u00f9 volte pres.) e del Consiglio di Stato di Appenzello Esterno (1910-19; Dip. dell'istruzione pubblica, degli affari comunali e della giustizia), nonch\u00e9 Consigliere nazionale (1921-31). Esponente del partito radicale, si oppose principalmente all'aumento della spesa pubblica della Conf. Fece parte dei consigli di amministrazione della Cassa ipotecaria del cant. San Gallo (dal 1917), delle Forze motrici di San Gallo-Appenzello (1914-31) e della linea tranviaria elettrica San Gallo-Gais-Appenzello (1920-46) e presiedette il consiglio di amministrazione dell'azienda di ricami Sonderegger & Co a Herisau (1920-31)."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q2645176", "itemLabel": "Alfred Hofstetter", "dhsid": "006382", "namefr": "", "articlefr": "", "namede": "Alfred Hofstetter (Politiker, 1869)", "articlede": "https://de.wikipedia.org/wiki/Alfred_Hofstetter_(Politiker,_1869)", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1059409771"}], "wikidata_url": "http://www.wikidata.org/entity/Q2645176", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/it/articles/006382/2006-11-07"}