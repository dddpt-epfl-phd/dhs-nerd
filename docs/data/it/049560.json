agrid_links": [{"Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=86959", "short_description": "Elites suisses au XXe si\u00e8cle", "long_description": "Elites suisses au XXe si\u00e8cle"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/172441528"}, {"title": "VIAF", "url": "http://viaf.org/viaf/195248942"}], "bref": [{"title": "Dati biografici", "text": "\u2217\ufe0e 13.12.1897\n   \u271d\ufe0e 10.3.1984\n1897-12-131984-03-10", "birth_date": "1897-12-13", "death_date": "1984-03-10"}], "birth_date": "1897-12-13", "death_date": "1984-03-10", "text_blocks": [["h1", "Emil Jakob Walter"], ["p", "13.12.1897 Winterthur, 10.3.1984 Zurigo, rif., di Winterthur, dal 1942 di Zurigo. Figlio di Emil (->) e di Marie H\u00fcni. \u200b1) (1923) Alexandra Kraus, figlia di Emil; 2) (1929) Rosa Marie Pohl, figlia di Wilhelm; 3) (1941) Annemarie Locher, figlia di August Gottfried Wilhelm Meyer. Dopo gli studi di chimica all'Univ. di Zurigo (1916-21), consegu\u00ec il dottorato (1924). Lavor\u00f2 dapprima come chimico presso la Aluminium Industrie Aktien Gesellschaft (dal 1963 Alusuisse) a Chippis. In seguito fu docente titolare di chimica e fisica alla scuola di arti e mestieri di Zurigo (1930-58), libero docente di storia delle scienze all'Univ. di Zurigo (1949-67), professore incaricato (1951) e professore straordinario (1957-68) di sociologia all'Univ. di San Gallo. Partendo da un rigido positivismo cerc\u00f2 tra altro di applicare i metodi delle scienze naturali alle problematiche sociologiche e psicologiche. Fu inoltre consigliere com. socialista della citt\u00e0 di Zurigo (1934-62)."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q17166462", "itemLabel": "Emil Jakob Walter", "dhsid": "049560", "namefr": "", "articlefr": "", "namede": "Emil J. Walter", "articlede": "https://de.wikipedia.org/wiki/Emil_J._Walter", "nameit": "", "articleit": "", "nameen": "Emil J. Walter", "articleen": "https://en.wikipedia.org/wiki/Emil_J._Walter", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "172441528"}], "wikidata_url": "http://www.wikidata.org/entity/Q17166462", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/it/articles/049560/2014-12-27"}