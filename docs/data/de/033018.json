metagrid_links": [{"Sikart": {"url": "http://www.sikart.ch/kuenstlerinnen.aspx?id=4025621", "short_description": "SIKART", "long_description": "SIKART"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/1053624956"}, {"title": "VIAF", "url": "http://viaf.org/viaf/309637107"}], "bref": [{"title": "Famili\u00e4re Zugeh\u00f6rigkeit", "text": "Keyser", "link": ["/articles/024106/2008-10-14/"]}, {"title": "Lebensdaten", "text": "\u2217\ufe0e 20.6.1813\n   \u271d\ufe0e 28.12.1900\n1813-06-201900-12-28", "birth_date": "1813-06-20", "death_date": "1900-12-28"}], "birth_date": "1813-06-20", "death_date": "1900-12-28", "text_blocks": [["h1", "Heinrich Keyser"], ["p", "20.6.1813 Stans, 28.12.1900 M\u00fcnchen, kath., von Stans. Sohn des Franz Josef, Kirchmeiers und Ratsherrn, und der Klara Josefa geb. Zelger. Bruder des Franz (->). 1843 Anna Zelger, Tochter des Klemenz Zelger. 1826-34 Besuch des Jesuitenkollegiums in Brig. Ausbildung zum Maler beim Schwager Lorenz Justin Ritz, 1834-36 Kunstakad. Mailand, 1836-37 Kunstakad. Rom. Dort gewann K. den 1. Preis der Academia di San Luca und erhielt einen Portr\u00e4tauftrag f\u00fcr Papst Gregor XVI. Nach einem Zwischenaufenthalt in Freiburg kehrte K. nach Stans zur\u00fcck und bet\u00e4tigte sich als Portr\u00e4t- und Kirchenmaler. 1847-50 wirkte er als Nidwaldner Landeshauptmann. K. zog sich nach 1850 aus der Politik zur\u00fcck und widmete sich ganz, abwechselnd in Stans und M\u00fcnchen arbeitend, seiner auch in Europa und den USA begehrten religi\u00f6sen Kunst. Mit Melchior Paul von Deschwanden unterhielt er eine enge Freundschaft."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q52154798", "itemLabel": "Heinrich Keyser", "dhsid": "033018", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1053624956"}], "wikidata_url": "http://www.wikidata.org/entity/Q52154798", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "K", "url": "https://hls-dhs-dss.ch/de/articles/033018/2008-10-16"}