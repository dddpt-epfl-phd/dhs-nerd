chweiz."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q94867816", "itemLabel": "Q94867816", "dhsid": "046490", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "136208746"}], "wikidata_url": "http://www.wikidata.org/entity/Q94867816", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "N", "url": "https://hls-dhs-dss.ch/de/articles/046490/2008-09-01"}