chafft."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q18412493", "itemLabel": "Q18412493", "dhsid": "048308", "namefr": "", "articlefr": "", "namede": "Hotz-Linder-Agreement", "articlede": "https://de.wikipedia.org/wiki/Hotz-Linder-Agreement", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "", "instanceofLabel": "", "gndid": ""}], "wikidata_url": "http://www.wikidata.org/entity/Q18412493", "wikipedia_page_title": "Hotz-Linder-Agreement", "added_wikidata_wikipedia_to_text_links": true, "_initial": "H", "url": "https://hls-dhs-dss.ch/de/articles/048308/2006-11-17"}