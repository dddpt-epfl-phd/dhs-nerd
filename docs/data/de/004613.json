rst."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q2645511", "itemLabel": "Alfred Moll", "dhsid": "004613", "namefr": "", "articlefr": "", "namede": "Alfred Moll", "articlede": "https://de.wikipedia.org/wiki/Alfred_Moll", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1052424066"}], "wikidata_url": "http://www.wikidata.org/entity/Q2645511", "wikipedia_page_title": "Alfred Moll", "added_wikidata_wikipedia_to_text_links": true, "_initial": "M", "url": "https://hls-dhs-dss.ch/de/articles/004613/2007-11-30"}