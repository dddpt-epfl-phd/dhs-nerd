, "metagrid_links": [{"Dodis": {"url": "http://dodis.ch/P39686", "short_description": "Diplomatische Dokumente der Schweiz", "long_description": "Diplomatische Dokumente der Schweiz"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/1048367320"}, {"title": "VIAF", "url": "http://viaf.org/viaf/307289921"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 13.2.1890\n   \u271d\ufe0e 31.12.1952\n1890-02-131952-12-31", "birth_date": "1890-02-13", "death_date": "1952-12-31"}], "birth_date": "1890-02-13", "death_date": "1952-12-31", "text_blocks": [["h1", "Fritz Wieser"], ["p", "13.2.1890 Hirzel, 31.12.1952 Basel, ref., dann konfessionslos, von Basel und Neunkirch. Sohn des Gottlieb, Pfarrers, und der Lydia geb. V\u00f6gelin. Bruder des Gottlob (->). \u200b1) 1920 Rosa B\u00fctschi, 2) 1944 Johanna Cresenza Seglias. Schulen in Hirzel, Z\u00fcrich und Basel, 1909 Matura. Stud. der Mathematik, Physik, Chemie und Philosophie in Basel, Berlin, G\u00f6ttingen und Marburg. 1911 Mittellehrerexamen, 1917 Promotion in Basel. 1919-30 Grossrat (1928-29 Pr\u00e4s.). 1921 trat W. mit der Mehrheit der SP Basel zur KP \u00fcber, war dort u.a. 1927-29 schweiz. Parteipr\u00e4sident und wurde 1928 ins Exekutivkomitee der Kommunist. Internationale gew\u00e4hlt. Er verlor ab 1929 alle Partei\u00e4mter und trat 1931 aus der KP aus. 1917-30 Redaktor des \"Basler Vorw\u00e4rts\", dann freier Journalist, im 2. Weltkrieg hauptamtlich mit dem genossenschaftl. Wohnungsbau besch\u00e4ftigt. 1937-52 Zentralpr\u00e4s. des Schweiz. Mieterverbands."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q78075213", "itemLabel": "Fritz Wieser", "dhsid": "014821", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1048367320"}], "wikidata_url": "http://www.wikidata.org/entity/Q78075213", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "W", "url": "https://hls-dhs-dss.ch/de/articles/014821/2015-11-17"}