ice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/1082220442"}, {"title": "VIAF", "url": "http://viaf.org/viaf/88145541731696601494"}], "bref": [{"title": "Dati biografici", "text": "\u2217\ufe0e 25.1.1872\n   \u271d\ufe0e 14.6.1940\n1872-01-251940-06-14", "birth_date": "1872-01-25", "death_date": "1940-06-14"}], "birth_date": "1872-01-25", "death_date": "1940-06-14", "text_blocks": [["h1", "Josef Werlen"], ["p", "25.1.1872 Wiler (L\u00f6tschen), 14.6.1940 Wiler, catt., di Wiler. Figlio di Joseph, insegnante, e di Anna Maria Brunner. Dopo i collegi a Briga e Stans, studi\u00f2 teol. a Sion (1892-93) e Innsbruck (1893-96). Ordinato sacerdote (1895), fu parroco di Saas-Fee (1896-97), insegnante al collegio di Briga (1897-98), cancelliere vescovile (1898-1901), parroco di Kippel (1901-14, con il titolo di priore) e di Leuk (1914-24), ispettore scolastico (1909-24), canonico del capitolo cattedrale (dal 1924) e insegnante di religione alla scuola magistrale di Sion (1924-30). Autore dei programmi di studio per la scuola elementare (1925) e del catechismo (1938), fu iniziatore fondatore e redattore del bollettino parrocchiale dell'alto Vallese (1924) e cofondatore della soc. del Walliser Jahrbuch (1931). Pioniere delle casse Raiffeisen nell'alto Vallese, apr\u00ec 40 ist. locali e fond\u00f2 la federazione cant. altovallesana delle casse Raiffeisen sviz. (1917, primo pres.). Nel 1935 venne eletto nel consiglio di vigilanza dell'Unione sviz. delle casse Raiffeisen."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q96413174", "itemLabel": "Josef Werlen", "dhsid": "030857", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1082220442"}], "wikidata_url": "http://www.wikidata.org/entity/Q96413174", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/it/articles/030857/2013-01-23"}