agrid_id": "14421", "metagrid_links": [{"Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=76308", "short_description": "Schweizerische Eliten im 20. Jahrhundert", "long_description": "Schweizerische Eliten im 20. Jahrhundert"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/117610674"}, {"title": "VIAF", "url": "http://viaf.org/viaf/32778387"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 10.9.1863\n   \u271d\ufe0e 24.3.1936\n1863-09-101936-03-24", "birth_date": "1863-09-10", "death_date": "1936-03-24"}], "birth_date": "1863-09-10", "death_date": "1936-03-24", "text_blocks": [["h1", "Josef Jadassohn"], ["p", "10.9.1863 Liegnitz (Schlesien), 24.3.1936 Z\u00fcrich, prot., Deutscher. Sohn des Meyer, Kaufmanns, und der Elise geb. Kohn. \u200bMargarethe Kern. Gymnasium in Liegnitz, Medizinstud. in G\u00f6ttingen, Breslau, Heidelberg und Berlin, 1887 Dr. med. in Breslau. Ohne vorherige Habilitation wurde J. an der Univ. Bern 1896 ao. Prof. f\u00fcr Dermatologie und Syphilis, 1904 o. Prof. 1917-32 Prof. an der Univ. Breslau; im Ruhestand in Z\u00fcrich. J. geh\u00f6rt zu den Grossen seines Faches. Mit zahlreichen Sch\u00fclern bearbeitete er die gesamte Dermatologie und Venerologie; mehrere Krankheitsbilder tragen seinen Namen. Als Forscher und Therapeut befasste er sich besonders mit den durch Infektion, Allergie und Stoffwechselst\u00f6rungen bedingten Hautkrankheiten sowie mit den Geschlechtskrankheiten und setzte sich als Sozialhygieniker f\u00fcr deren Bek\u00e4mpfung ein."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q84945", "itemLabel": "Josef Jadassohn", "dhsid": "014421", "namefr": "", "articlefr": "", "namede": "Josef Jadassohn", "articlede": "https://de.wikipedia.org/wiki/Josef_Jadassohn", "nameit": "", "articleit": "", "nameen": "Josef Jadassohn", "articleen": "https://en.wikipedia.org/wiki/Josef_Jadassohn", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "117610674"}], "wikidata_url": "http://www.wikidata.org/entity/Q84945", "wikipedia_page_title": "Josef Jadassohn", "added_wikidata_wikipedia_to_text_links": true, "_initial": "J", "url": "https://hls-dhs-dss.ch/de/articles/014421/2005-04-19"}