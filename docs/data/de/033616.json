schweiz ein."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q1734657", "itemLabel": "Willi Wenk", "dhsid": "033616", "namefr": "", "articlefr": "", "namede": "Willi Wenk", "articlede": "https://de.wikipedia.org/wiki/Willi_Wenk", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "108990956X"}], "wikidata_url": "http://www.wikidata.org/entity/Q1734657", "wikipedia_page_title": "Willi Wenk", "added_wikidata_wikipedia_to_text_links": true, "_initial": "W", "url": "https://hls-dhs-dss.ch/de/articles/033616/2012-06-27"}