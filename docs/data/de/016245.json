i_links": [{"item": "http://www.wikidata.org/entity/Q96368245", "itemLabel": "Jean Georges Pillichody", "dhsid": "016245", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "142664901"}], "wikidata_url": "http://www.wikidata.org/entity/Q96368245", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "P", "url": "https://hls-dhs-dss.ch/de/articles/016245/2012-03-29"}