etagrid_links": [{"Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=63822", "short_description": "Schweizerische Eliten im 20. Jahrhundert", "long_description": "Schweizerische Eliten im 20. Jahrhundert"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/116251344"}, {"title": "VIAF", "url": "http://viaf.org/viaf/54895224"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 11.11.1834\n   \u271d\ufe0e 2.10.1918\n1834-11-111918-10-02", "birth_date": "1834-11-11", "death_date": "1918-10-02"}], "birth_date": "1834-11-11", "death_date": "1918-10-02", "text_blocks": [["h1", "Pierre-Louis Dunant"], ["p", "11.11.1834 Le Petit-Saconnex (heute Gem. Genf), 2.10.1918 Cologny, ref., von Genf. Sohn des Jean-Jacques, Kaufmanns, und der Anne Antoinette geb. Colladon. Bruder des Henry (->). 1865 Andr\u00e9e-Mathilde Colladon, Tochter des Jean-Daniel Colladon. 1854-60 Medizinstud. in Paris, 1863 Doktorat und Zulassung zur Soci\u00e9t\u00e9 m\u00e9dicale de Gen\u00e8ve. Ab 1873 stellvertretender Prof. f\u00fcr Hygiene an der naturwissenschaftl. Fakult\u00e4t der Univ. Genf. Ab 1876, dem Jahr der Schaffung der medizin. Fakult\u00e4t, bis 1889 o. Prof. Ausserdem Gr\u00fcnder und Arzt des Erholungsheims f\u00fcr Frauen in Pressy (Gem. Vand\u0153uvres). Gemeinsam mit Jean-Henri-Adolphe d'Espine verfasste D. 1881 einen Entwurf f\u00fcr das 1886 in Kraft gesetzte Bundesgesetz gegen gemeingef\u00e4hrliche Epidemien. Zudem schrieb er zahlreiche Werke und Artikel \u00fcber Hygiene, Demografie, medizin. Statistik und Epidemien."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q78060871", "itemLabel": "Pierre-Louis Dunant", "dhsid": "014713", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "116251344"}], "wikidata_url": "http://www.wikidata.org/entity/Q78060871", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "D", "url": "https://hls-dhs-dss.ch/de/articles/014713/2004-07-30"}