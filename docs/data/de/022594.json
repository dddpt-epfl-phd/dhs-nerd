etagrid_links": [{"BSG": {"url": "https://www.bsg.nb.admin.ch/discovery/search?query=lds50,contains,118940600&vid=41SNL_54_INST:bsg", "short_description": "BSG", "long_description": "Bibliographie der Schweizergeschichte"}, "Sikart": {"url": "http://www.sikart.ch/kuenstlerinnen.aspx?id=4022944", "short_description": "SIKART", "long_description": "SIKART"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/118940600"}, {"title": "VIAF", "url": "http://viaf.org/viaf/52488594"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 12.4.1806\n   \u271d\ufe0e 12.8.1834\n1806-04-121834-08-12", "birth_date": "1806-04-12", "death_date": "1834-08-12"}], "birth_date": "1806-04-12", "death_date": "1834-08-12", "text_blocks": [["h1", "Peter Rindisbacher"], ["p", "12.4.1806 Eggiwil, 12.8.1834 St. Louis (Missouri), prot., von Lauperswil. Sohn des Peter, Bauern und Tierarztes, und der Anna-Barbara geb. Wyss. Ledig. W\u00e4hrend Malerfahrten ins Berner Oberland Unterricht beim Maler Samuel Weibel. 1821 Auswanderung der Fam. nach Amerika. Reiseskizzen von Schiffen auf hoher See und im Eismeer sowie von Eskimos, auch von der Route vom Hudson Bay bis zur Red-River-Kolonie (heute Winnipeg, Kanada). 1826 \u00dcbersiedlung der Fam. nach Gratiot's Grove (Wisconsin). 1829 liess sich R., der als Maler und Portr\u00e4tist lebte, in St. Louis nieder. Er pflegte freundschaftl. Kontakte zu Mitgliedern versch. Indianerst\u00e4mme (Cree, Ojibwa, Assiniboin, Sioux, Sauk und Fox), deren Alltag (Fischen, Jagen und K\u00e4mpfen) er in Zeichnungen und Aquarellen festhielt. Ausserdem erstellte er Bildnisse von Freunden und Bekannten. R. gilt als Vorl\u00e4ufer des bekannteren \"Indianermalers\" Karl Bodmer. Sein Werk zeichnet sich durch seinen hohen dokumentar. Wert und Detailtreue aus."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q7176604", "itemLabel": "Peter Rindisbacher", "dhsid": "022594", "namefr": "Peter Rindisbacher", "articlefr": "https://fr.wikipedia.org/wiki/Peter_Rindisbacher", "namede": "Peter Rindisbacher", "articlede": "https://de.wikipedia.org/wiki/Peter_Rindisbacher", "nameit": "", "articleit": "", "nameen": "Peter Rindisbacher", "articleen": "https://en.wikipedia.org/wiki/Peter_Rindisbacher", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "118940600"}], "wikidata_url": "http://www.wikidata.org/entity/Q7176604", "wikipedia_page_title": "Peter Rindisbacher", "added_wikidata_wikipedia_to_text_links": true, "_initial": "R", "url": "https://hls-dhs-dss.ch/de/articles/022594/2010-11-29"}