gesandter."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q96367580", "itemLabel": "Josef Ignaz Xaver (von Heidegg) Pfyffer", "dhsid": "014465", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1051785952"}], "wikidata_url": "http://www.wikidata.org/entity/Q96367580", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/de/articles/014465/2009-08-05"}