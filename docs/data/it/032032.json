ca."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q55842924", "itemLabel": "Georg Kahlbaum", "dhsid": "032032", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "116021020"}], "wikidata_url": "http://www.wikidata.org/entity/Q55842924", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/it/articles/032032/2006-11-02"}