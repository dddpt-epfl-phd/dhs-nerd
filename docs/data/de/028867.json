t)."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q55673729", "itemLabel": "Henri Ladame", "dhsid": "028867", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "104187077"}], "wikidata_url": "http://www.wikidata.org/entity/Q55673729", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "L", "url": "https://hls-dhs-dss.ch/de/articles/028867/2009-03-10"}