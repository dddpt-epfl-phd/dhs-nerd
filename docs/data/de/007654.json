amefr": "", "articlefr": "", "namede": "Busskirch", "articlede": "https://de.wikipedia.org/wiki/Busskirch", "nameit": "", "articleit": "", "nameen": "Busskirch", "articleen": "https://en.wikipedia.org/wiki/Busskirch", "instanceof": "http://www.wikidata.org/entity/Q532", "instanceofLabel": "village", "gndid": ""}], "wikidata_url": "http://www.wikidata.org/entity/Q692055", "wikipedia_page_title": "Busskirch", "added_wikidata_wikipedia_to_text_links": true, "_initial": "B", "url": "https://hls-dhs-dss.ch/de/articles/007654/2017-03-16"}