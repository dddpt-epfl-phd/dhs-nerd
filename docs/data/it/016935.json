d_id": "16935", "metagrid_links": [{}], "notice_links": [{"title": "e-LIR", "url": "http://e-lir.ch/index.php?id=2260"}, {"title": "GND", "url": "http://d-nb.info/gnd/1075262542"}, {"title": "VIAF", "url": "http://viaf.org/viaf/317061541"}], "bref": [{"title": "Dati biografici", "text": "\u2217\ufe0e 10.9.1702\n   \u271d\ufe0e 22.8.1765\n1702-09-101765-08-22", "birth_date": "1702-09-10", "death_date": "1765-08-22"}], "birth_date": "1702-09-10", "death_date": "1765-08-22", "text_blocks": [["h1", "Anton vonSalis Soglio"], ["p", "10.9.1702 Soglio (oggi com. Bregaglia), 22.8.1765 Coira, rif., di Coira, Soglio e Bivio. Figlio di Baptista, commissario. \u200bMargarethe, figlia di Peter (->). Podest\u00e0 della Bregaglia (1725 e 1726), Consigliere a Coira e giudice cittadino, fu pres. della Lega Caddea (1751-55, 1757-61 e 1762-64). Dapprima colonnello e proprietario di un reggimento grigionese al servizio della Spagna, pass\u00f2 poi alla Francia. Nella lite del 1755 relativa all'elezione del vescovo, si schier\u00f2 dalla parte della Lega Caddea. Nonostante i suoi sforzi per raggiungere un concordato con il papa fallirono, grazie al suo impegno nella realizzazione del terzo capitolato di Milano, divenne uno dei politici grigionesi di maggiore successo del XVIII sec."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q27887750", "itemLabel": "Q27887750", "dhsid": "016935", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1089917716"}], "wikidata_url": "http://www.wikidata.org/entity/Q27887750", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/it/articles/016935/2012-01-06"}