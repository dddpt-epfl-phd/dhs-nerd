lin."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q87845", "itemLabel": "Leonard Steckel", "dhsid": "009247", "namefr": "", "articlefr": "", "namede": "Leonard Steckel", "articlede": "https://de.wikipedia.org/wiki/Leonard_Steckel", "nameit": "", "articleit": "", "nameen": "Leonard Steckel", "articleen": "https://en.wikipedia.org/wiki/Leonard_Steckel", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "117224685"}], "wikidata_url": "http://www.wikidata.org/entity/Q87845", "wikipedia_page_title": "Leonard Steckel", "added_wikidata_wikipedia_to_text_links": true, "_initial": "S", "url": "https://hls-dhs-dss.ch/de/articles/009247/2012-11-07"}