metagrid_links": [{"Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=65920", "short_description": "Schweizerische Eliten im 20. Jahrhundert", "long_description": "Schweizerische Eliten im 20. Jahrhundert"}, "BSG": {"url": "https://www.bsg.nb.admin.ch/discovery/search?query=lds50,contains,119318229&vid=41SNL_54_INST:bsg", "short_description": "BSG", "long_description": "Bibliographie der Schweizergeschichte"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/119318229"}, {"title": "VIAF", "url": "http://viaf.org/viaf/24867136"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 20.11.1921\n   \u271d\ufe0e 17.11.1998\n1921-11-201998-11-17", "birth_date": "1921-11-20", "death_date": "1998-11-17"}], "birth_date": "1921-11-20", "death_date": "1998-11-17", "text_blocks": [["h1", "Norbert Moret"], ["p", "20.11.1921 M\u00e9ni\u00e8res, 17.11.1998 Freiburg, von M\u00e9ni\u00e8res. Sohn des Joseph Paul, Landwirts und vermutlich Posthalters, und der Anna Laurette geb. M. Bruder des Marc (->). 1948 Germaine Louise Fivaz. Nach einem Musikstud. in Freiburg bildete sich M. 1948-50 in Paris bei Arthur Honegger, Olivier Messiaen und Ren\u00e9 Leibowitz sowie 1950-51 bei Paul Kletzki und Wilhelm Furtw\u00e4ngler in Wien fort. Ab 1952 wirkte er als Musiklehrer in Freiburg. Nach der Urauff\u00fchrung seines Chorwerks \"Germes en \u00e9veil\" 1974 widmete er sich mit der Unterst\u00fctzung von Paul Sacher und Mstislaw Rostropowitsch ganz der Komposition. Die Urauff\u00fchrungen seiner \"Hymnes de silence\" 1978 durch Sacher in Basel, des Violinkonzerts durch Anne-Sophie Mutter und des Cellokonzerts durch Rostropowitsch machten M. international bekannt. Die klangl. Verwirklichung eines inneren Bildes bildete den Kern seines kompositor. Credos. 1983 Kompositionspreis des Schweiz. Tonk\u00fcnstlervereins und Europapreis f\u00fcr Musik, 1989 Ehrendoktor der Univ. Freiburg."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q3343505", "itemLabel": "Norbert Moret", "dhsid": "020711", "namefr": "Norbert Moret", "articlefr": "https://fr.wikipedia.org/wiki/Norbert_Moret", "namede": "Norbert Moret", "articlede": "https://de.wikipedia.org/wiki/Norbert_Moret", "nameit": "", "articleit": "", "nameen": "Norbert Moret", "articleen": "https://en.wikipedia.org/wiki/Norbert_Moret", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "119318229"}], "wikidata_url": "http://www.wikidata.org/entity/Q3343505", "wikipedia_page_title": "Norbert Moret", "added_wikidata_wikipedia_to_text_links": true, "_initial": "M", "url": "https://hls-dhs-dss.ch/de/articles/020711/2009-01-09"}