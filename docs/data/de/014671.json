grid_id": "14671", "metagrid_links": [{"Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=76505", "short_description": "Schweizerische Eliten im 20. Jahrhundert", "long_description": "Schweizerische Eliten im 20. Jahrhundert"}, "BSG": {"url": "https://www.bsg.nb.admin.ch/discovery/search?query=lds50,contains,119034654&vid=41SNL_54_INST:bsg", "short_description": "BSG", "long_description": "Bibliographie der Schweizergeschichte"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/119034654"}, {"title": "VIAF", "url": "http://viaf.org/viaf/54949147"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 17.10.1856\n   \u271d\ufe0e 2.12.1939\n1856-10-171939-12-02", "birth_date": "1856-10-17", "death_date": "1939-12-02"}], "birth_date": "1856-10-17", "death_date": "1939-12-02", "text_blocks": [["h1", "Alexander Tschirch"], ["p", "17.10.1856 Guben (Niederlausitz), 2.12.1939 Bern, ref., aus Deutschland. Sohn des Adolf, Pfarrers, und der Maria geb. Sausse. 1885 Elise Ziurek, Tochter des Otto, Apothekers. Apothekerlehre in Loschwitz bei Dresden. Ab 1877 u.a. in der Berner Staatsapotheke, 1878-80 Pharmaziestud. in Berlin, 1881 Promotion in Freiburg i.Br., 1884 PD f\u00fcr Botanik in Berlin, 1888-89 Studienreise nach Indien, Ceylon und Java. 1890-1932 Prof. f\u00fcr Pharmakognosie, pharmazeut. und gerichtl. Chemie an der Univ. Bern (1908-09 Rektor). T. machte das Berner pharmazeut. Institut zu einer Hochburg der akadem. Pharmazie. Er betreute \u00fcber 150 Dissertationen, darunter viele von ausl\u00e4nd. Studenten. Sein wissenschaftl. Werk umfasst 20 B\u00fccher und 400 Zeitschriftenaufs\u00e4tze. Sein Interesse galt der Pflanzenanatomie, Harzen und Antrachinonglykosiden. Als Leiter der eidg. Pharmakop\u00f6ekommission verfasste T. auch Beitr\u00e4ge zur Arzneimittelpr\u00fcfung. Er wirkte massgebend an der Bearbeitung der vierten und der f\u00fcnften Ausgabe der \"Pharmacopoea Helvetica\" mit. T. erhielt sechs Ehrendoktorate und war Ehrenmitglied von ca. 40 wissenschaftl. Gesellschaften."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q75096", "itemLabel": "Alexander Tschirch", "dhsid": "014671", "namefr": "", "articlefr": "", "namede": "Alexander Tschirch", "articlede": "https://de.wikipedia.org/wiki/Alexander_Tschirch", "nameit": "Alexander Tschirch", "articleit": "https://it.wikipedia.org/wiki/Alexander_Tschirch", "nameen": "Alexander Tschirch", "articleen": "https://en.wikipedia.org/wiki/Alexander_Tschirch", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "119034654"}], "wikidata_url": "http://www.wikidata.org/entity/Q75096", "wikipedia_page_title": "Alexander Tschirch", "added_wikidata_wikipedia_to_text_links": true, "_initial": "T", "url": "https://hls-dhs-dss.ch/de/articles/014671/2014-01-07"}