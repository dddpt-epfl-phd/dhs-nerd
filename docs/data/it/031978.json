tudi": [{"text": "Livre du Recteur, 3, 284", "tpub": ["Livre du Recteur"]}, {"text": "J. Trembley (a cura di), Les savants genevois dans l'Europe intellectuelle du XVIIe au milieu du XIXe si\u00e8cle, 1987, 256-262, 402", "author": ["J. Trembley (a cura di)"], "tpub": ["Les savants genevois dans l'Europe intellectuelle du XVIIe au milieu du XIXe si\u00e8cle"]}]}, "metagrid_id": "31978", "metagrid_links": [{"Dodis": {"url": "http://dodis.ch/P33499", "short_description": "Documenti diplomatici svizzeri", "long_description": "Documenti diplomatici svizzeri"}, "Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=62981", "short_description": "Elites suisses au XXe si\u00e8cle", "long_description": "Elites suisses au XXe si\u00e8cle"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/117503215"}, {"title": "VIAF", "url": "http://viaf.org/viaf/2771532"}], "bref": [{"title": "Appartenenza familiare", "text": "Favre", "link": ["/articles/025507/2004-11-23/"]}, {"title": "Dati biografici", "text": "\u2217\ufe0e 31.3.1815\n   \u271d\ufe0e 11.7.1890\n1815-03-311890-07-11", "birth_date": "1815-03-31", "death_date": "1890-07-11"}], "birth_date": "1815-03-31", "death_date": "1890-07-11", "text_blocks": [["h1", "Alphonse Favre"], ["p", "31.3.1815 Ginevra, 11.7.1890 Pregny, rif., di Ginevra. Figlio di Guillaume, ufficiale e storico, e di Catherine Bertrand. Fratello di Edmond (->). \u200b(1844) Henriette-Julie, figlia di Jean-Jacques Rigaud. Studi\u00f2 scienze naturali e diritto all'Acc. di Ginevra (1831-37), poi chimica e mineralogia a Parigi (1839). Basandosi sull'opera di Horace B\u00e9n\u00e9dict de Saussure, F. comp\u00ec ricerce sulla geologia della regione del Monte Bianco, del Sal\u00e8ve e del cant. Ginevra, di cui disegn\u00f2 le prime carte geologiche. Stabil\u00ec con precisione l'estensione degli antichi ghiacciai e organizz\u00f2 il censimento e la protezione dei massi erratici sia in Svizzera sia nelle regioni confinanti della Francia. Fra il 1844 e il 1852 insegn\u00f2 geologia e paleontologia all'Acc. di Ginevra. Fu cofondatore (1865) e pres. (1866) del Club alpino sviz."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q1542089", "itemLabel": "Alphonse Favre", "dhsid": "031978", "namefr": "", "articlefr": "", "namede": "Alphonse Favre", "articlede": "https://de.wikipedia.org/wiki/Alphonse_Favre", "nameit": "", "articleit": "", "nameen": "Alphonse Favre", "articleen": "https://en.wikipedia.org/wiki/Alphonse_Favre", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "117503215"}], "wikidata_url": "http://www.wikidata.org/entity/Q1542089", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "F", "url": "https://hls-dhs-dss.ch/it/articles/031978/2006-03-28"}