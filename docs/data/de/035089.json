ks": [{"item": "http://www.wikidata.org/entity/Q2960404", "itemLabel": "Charles Vuillermet", "dhsid": "035089", "namefr": "Charles Vuillermet", "articlefr": "https://fr.wikipedia.org/wiki/Charles_Vuillermet", "namede": "Charles Vuillermet", "articlede": "https://de.wikipedia.org/wiki/Charles_Vuillermet", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1072172747"}], "wikidata_url": "http://www.wikidata.org/entity/Q2960404", "wikipedia_page_title": "Charles Vuillermet", "added_wikidata_wikipedia_to_text_links": true, "_initial": "V", "url": "https://hls-dhs-dss.ch/de/articles/035089/2013-03-25"}