otice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/107422101X"}, {"title": "VIAF", "url": "http://viaf.org/viaf/316771804"}], "bref": [{"title": "Famili\u00e4re Zugeh\u00f6rigkeit", "text": "Imhof (BE)", "link": ["/articles/020884/2005-07-04/"]}, {"title": "Lebensdaten", "text": "\u2248\ufe0e 6.2.1596\n   \u271d\ufe0e 20.12.1657\n1657-12-20", "death_date": "1657-12-20"}], "death_date": "1657-12-20", "text_blocks": [["h1", "Hans Georg Imhof"], ["p", "6.2.1596 Aarau, 20.12.1657 Lenzburg, reformiert, von Aarau, ab 1617 Burger von Bern. Sohn des Hans Georg, Schultheissen von Aarau. \u200b1) 1623 Ursula Lerber, Tochter des Daniel Lerber, 2) Elisabeth Horn, Witwe des Samuel Freudenreich, Berner Kleinrats. Ab 1613 Studium an der Universit\u00e4t Herborn (Hessen). Notar. 1617 Eintritt in den Staatsdienst als Kanzleisubstitut. 1624 Grossrat, 1626-1629 Gerichtsschreiber, 1629-1635 Landvogt von Wangen an der Aare, 1642-1648 Hofmeister von K\u00f6nigsfelden, 1656-1657 Landvogt von Lenzburg. Als Gesandter im Thuner Aufstand von 1641 und im Bauernkrieg von 1653 nahm Hans Georg Imhof, der selbst aus einer bernischen Untertanenstadt stammte, eine vermittelnde Position ein."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q96418270", "itemLabel": "Hans Georg Imhof", "dhsid": "016740", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "107422101X"}], "wikidata_url": "http://www.wikidata.org/entity/Q96418270", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/de/articles/016740/2008-01-22"}