75)."]], "text_links": [[{"start": 0, "end": 18, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "PERSON", "extra_fields": {"rawName": "dialogue \u0153cum\u00e9nique", "confidence_score": 0.9683, "domains": ["Biology", "Sociology"], "origin": "entity_fishing"}}}], [{"start": 12, "end": 19, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "LOCATION", "extra_fields": {"rawName": "dialogue \u0153cum\u00e9nique", "confidence_score": 0.9683, "domains": ["Biology", "Sociology"], "origin": "entity_fishing"}}}, {"start": 57, "end": 63, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q7024", "wikipedia_page_id": 136602, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q7024", "grobid_tag": null, "extra_fields": {"rawName": "dialogue \u0153cum\u00e9nique", "confidence_score": 0.9683, "domains": ["Biology", "Sociology"], "origin": "entity_fishing"}}, "href": "fr/articles/002177", "dhsid": "002177", "wiki": {"item": "http://www.wikidata.org/entity/Q7024", "itemLabel": "Lugano", "dhsid": "002177", "namefr": "Lugano", "articlefr": "https://fr.wikipedia.org/wiki/Lugano", "namede": "Lugano", "articlede": "https://de.wikipedia.org/wiki/Lugano", "nameit": "Lugano", "articleit": "https://it.wikipedia.org/wiki/Lugano", "nameen": "Lugano", "articleen": "https://en.wikipedia.org/wiki/Lugano", "instanceof": "http://www.wikidata.org/entity/Q515", "instanceofLabel": "ville", "gndid": "4099915-4", "wikidata_id": "Q7024"}}, {"start": 75, "end": 97, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "PERSON", "extra