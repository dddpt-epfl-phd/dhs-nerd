nks": [{"title": "GND", "url": "http://d-nb.info/gnd/138780609"}, {"title": "VIAF", "url": "http://viaf.org/viaf/266846559"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 27.6.1780\n   \u271d\ufe0e 9.3.1859\n1780-06-271859-03-09", "birth_date": "1780-06-27", "death_date": "1859-03-09"}], "birth_date": "1780-06-27", "death_date": "1859-03-09", "text_blocks": [["h1", "Josef Anton Berchtold"], ["p", "27.6.1780 Greich, 9.3.1859 Sitten, kath., von Greich. Sohn des Peter, Landwirts. 1792-98 Kollegium Brig, 1798-1803 Priesterseminar Sitten. 1803-16 Pfarrer von Leukerbad, 1816-19 von Sitten. 1807-11 Direktor des bisch\u00f6fl. Seminars, 1809 Titulardomherr, 1816 Kapitular, 1816-26 Poenitentiar, 1830-59 Promotor (Kirchenanwalt) im Unterwallis, 1837-59 Dekan von Valeria. Gr\u00fcnder der Volksschulen Leukerbad und Sitten, Mitverfasser des Schulgesetzes von 1827. Erste Triangulation des Wallis, 1829 privat begonnen, nach 1834 im Auftrag der Eidgenossenschaft (Grundlage der Dufourkarte). B. organisierte Hilfssammlungen (Rhonetal\u00fcberschwemmung 1839, Dorfbr\u00e4nde von Nax und Betten) und regte die Schaffung einer Versicherungsanstalt und einer Schuld-Tilgungsbank (1853) an."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q15432837", "itemLabel": "Joseph Anton Berchtold", "dhsid": "026604", "namefr": "", "articlefr": "", "namede": "Josef Anton Berchtold", "articlede": "https://de.wikipedia.org/wiki/Josef_Anton_Berchtold", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "138780609"}], "wikidata_url": "http://www.wikidata.org/entity/Q15432837", "wikipedia_page_title": "Josef Anton Berchtold", "added_wikidata_wikipedia_to_text_links": true, "_initial": "B", "url": "https://hls-dhs-dss.ch/de/articles/026604/2002-06-25"}