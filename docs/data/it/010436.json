rchivi": [{"text": "Fondo fotografico presso Ist. di archeologia e di scienze dell'antichit\u00e0 dell'Univ. di Losanna"}], "Studi": [{"text": "A. Bielman, Histoire de l'histoire ancienne et de l'arch\u00e9ologie \u00e0 l'Universit\u00e9 de Lausanne 1537-1987, 1987, 83-98", "author": ["A. Bielman"], "tpub": ["Histoire de l'histoire ancienne et de l'arch\u00e9ologie \u00e0 l'Universit\u00e9 de Lausanne 1537-1987"]}, {"text": "Professeurs Lausanne, 261 sg.", "tpub": ["Professeurs Lausanne"]}]}, "metagrid_id": "10436", "metagrid_links": [{"Dodis": {"url": "https://dodis.ch/P66194", "short_description": "Documenti diplomatici svizzeri", "long_description": "Documenti diplomatici svizzeri"}, "Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=77080", "short_description": "Elites suisses au XXe si\u00e8cle", "long_description": "Elites suisses au XXe si\u00e8cle"}, "BSS": {"url": "https://www.bsg.nb.admin.ch/discovery/search?query=lds50,contains,122792947&vid=41SNL_54_INST:bsg", "short_description": "Bibliografia della storia svizzera", "long_description": "Bibliografia della storia svizzera"}}], "notice_links": [{"title": "fotoCH", "url": "https://it.foto-ch.ch/#/home#detail=30744&type=photographer"}, {"title": "GND", "url": "http://d-nb.info/gnd/122792947"}, {"title": "VIAF", "url": "http://viaf.org/viaf/110380267"}], "bref": [{"title": "Dati biografici", "text": "\u2217\ufe0e 19.4.1902\n   \u271d\ufe0e 8.9.1981\n1902-04-191981-09-08", "birth_date": "1902-04-19", "death_date": "1981-09-08"}], "birth_date": "1902-04-19", "death_date": "1981-09-08", "text_blocks": [["h1", "Paul Collart"], ["p", "19.4.1902 Ginevra, 8.9.1981 Ginevra, rif., di Ginevra. Figlio di Louis, architetto. \u200bMadeleine Mansion, figlia di Auguste, chimico. Consegu\u00ec la licenza (1924) e il dottorato in lettere (1937) a Ginevra. Membro straniero della scuola franc. di Atene dal 1926, comp\u00ec scavi archeologici a Baalbek (Libano) dal 1938 al 1940. Fu professore straordinario (1946-50) e ordinario (1950-63) di archeologia e storia antica a Losanna, e professore straordinario (1948-50) e ordinario di storia antica a Ginevra (1950-63). Nel 1953 l'UNESCO gli affid\u00f2 l'inventario dei beni culturali della Siria e del Libano. Dal 1954 al 1956 diresse a Palmira (Siria) i primi grandi scavi sviz. all'estero. Fu direttore dell'Ist. sviz. di Roma (1963-70). Durante i suoi viaggi attorno al Mediterraneo (1928-70) scatt\u00f2 migliaia di fotografie. Gli venne conferita la medaglia Georges Perrot dell'Acad\u00e9mie des inscriptions e belles-lettres di Parigi (1971)."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q60642941", "itemLabel": "Paul Collart", "dhsid": "010436", "namefr": "", "articlefr": "", "namede": "Paul Collart", "articlede": "https://de.wikipedia.org/wiki/Paul_Collart", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "122792947"}], "wikidata_url": "http://www.wikidata.org/entity/Q60642941", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/it/articles/010436/2005-08-03"}