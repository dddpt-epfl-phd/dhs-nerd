ce_links": [{"title": "GND", "url": "http://d-nb.info/gnd/131770462"}, {"title": "VIAF", "url": "http://viaf.org/viaf/55289518"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 29.4.1920\n   \u271d\ufe0e 27.2.2005\n1920-04-292005-02-27", "birth_date": "1920-04-29", "death_date": "2005-02-27"}], "birth_date": "1920-04-29", "death_date": "2005-02-27", "text_blocks": [["h1", "Armand Forel"], ["p", "29.4.1920 Bern,27.2.2005 Nyon, konfessionslos, von Morges. Sohn des Oscar (->). Enkel des Auguste (->). \u200b1) Ren\u00e9e Freies, 2) Madeleine Gumy. Mittelschulbildung in versch. \u00f6ffentlichen und privaten Schulen. Medizinstudium in Lausanne. Ab 1950 Allgemeinpraktiker in Nyon. Als Antifaschist trat F. 1943 der 1940 verbotenen Kommunist. Partei bei und war von 1945 an Mitglied der Partei der Arbeit. Fr\u00fch \u00fcbernahm er polit. \u00c4mter: 1945-88 war er Grossrat, 1947-51 und 1955-80 Nationalrat, 1957-61 und 1981-89 Mitglied des Munizipalrats (Exekutive) von Nyon. F. setzte sich v.a. f\u00fcr soziale Anliegen ein und k\u00fcmmerte sich um Fragen, welche die Armee betrafen. In der Umgebung von Nyon wurde er als Arzt der Armen bekannt."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q677418", "itemLabel": "Armand Forel", "dhsid": "004940", "namefr": "Armand Forel", "articlefr": "https://fr.wikipedia.org/wiki/Armand_Forel", "namede": "Armand Forel", "articlede": "https://de.wikipedia.org/wiki/Armand_Forel", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "131770462"}], "wikidata_url": "http://www.wikidata.org/entity/Q677418", "wikipedia_page_title": "Armand Forel", "added_wikidata_wikipedia_to_text_links": true, "_initial": "F", "url": "https://hls-dhs-dss.ch/de/articles/004940/2005-04-27"}