etagrid_links": [{"parl.ch": {"url": "https://www.parlament.ch/de/biografie?CouncillorId=2821", "short_description": "Members of the Swiss parliament", "long_description": "Members of the Swiss National Council and the Council of States since 1848"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/1051071658"}, {"title": "VIAF", "url": "http://viaf.org/viaf/308708790"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 24.7.1831\n   \u271d\ufe0e 10.9.1880\n1831-07-241880-09-10", "birth_date": "1831-07-24", "death_date": "1880-09-10"}], "birth_date": "1831-07-24", "death_date": "1880-09-10", "text_blocks": [["h1", "Paul Nagel"], ["p", "24.7.1831 Engishofen (Gem. Erlen), 10.9.1880 Bischofszell, ref., von Engishofen. Sohn des Johann Konrad und der Anna Katharina geb. Haffter. 1867 Marie Barbara Pol, Tochter des Lorenz, Landwirts. Schwager des Jakob Albert Scherb. Rechtsstud. in Z\u00fcrich und Heidelberg, 1854 thurg. Anwaltspatent. Anwalt in Weinfelden. Mit Philipp Gottlieb Labhardt und Fridolin Anderwert Mitglied des liberalen Vereins von 1858, sp\u00e4ter der demokrat. Opposition. 1861-66 Vizestaatsanwalt unter Eduard H\u00e4berlin, 1863-80 Bezirksgerichtspr\u00e4s. von Bischofszell, 1869-80 Ersatzoberrichter. 1858-80 thurg. Kantonsrat (1869-70 Pr\u00e4s.), 1863 erfolglose Nationalratskandidatur, 1868-69 Mitglied des kant. Verfassungsrats, 1869-80 St\u00e4nderat (demokrat. Linke, 1876-77 Pr\u00e4s.). Gegner der Gotthardbahn, Hauptinitiant der Linie Sulgen-Bischofszell-Gossau (SG, 1876). Bef\u00fcrworter der Revision der Bundesverfassung von 1872 und 1874. Versch. Gemeinde\u00e4mter in Bischofszell. 1861-67 Mitglied der evang. Synode. 1869-80 Pr\u00e4s. des parit\u00e4t. Administrativrats. Oberstleutnant der Infanterie."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q16832467", "itemLabel": "Paul Nagel", "dhsid": "004081", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "Paul Nagel (politician)", "articleen": "https://en.wikipedia.org/wiki/Paul_Nagel_(politician)", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": ""}], "wikidata_url": "http://www.wikidata.org/entity/Q16832467", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/de/articles/004081/2009-06-22"}