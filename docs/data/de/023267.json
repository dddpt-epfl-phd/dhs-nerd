etagrid_id": "23267", "metagrid_links": [{"BSG": {"url": "https://www.bsg.nb.admin.ch/discovery/search?query=lds50,contains,132874105&vid=41SNL_54_INST:bsg", "short_description": "BSG", "long_description": "Bibliographie der Schweizergeschichte"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/132874105"}, {"title": "VIAF", "url": "http://viaf.org/viaf/70102591"}], "bref": [{"title": "Famili\u00e4re Zugeh\u00f6rigkeit", "text": "J\u00e4ggli", "link": ["/articles/023864/2008-01-28/"]}, {"title": "Lebensdaten", "text": "\u2217\ufe0e 2.5.1880\n   \u271d\ufe0e 27.12.1959\n1880-05-021959-12-27", "birth_date": "1880-05-02", "death_date": "1959-12-27"}], "birth_date": "1880-05-02", "death_date": "1959-12-27", "text_blocks": [["h1", "Mario J\u00e4ggli"], ["p", "2.5.1880 Bellinzona, 27.12.1959 Lugano, von Winterthur. Sohn des Christian August, Eisenbahnangestellten, und der Maria geb. Domenighetti. 1908 Giovannina Maina, von Caslano. 1900-05 Stud. der Naturwissenschaften an der Univ. Z\u00fcrich, 1908 Doktorat. P\u00e4dagogiklehrer am Lehrerseminar von Locarno, 1907-15 dessen Rektor. Ab 1915 Lehrer f\u00fcr Warenkunde an der Handelsschule von Bellinzona, 1922-47 dort Rektor. 1927-29 Pr\u00e4s. der Tessiner Gesellschaft f\u00fcr Naturwissenschaften. Als Sozialist und Antifaschist 1937 Gr\u00fcndungsmitglied des Verlags Nuove Edizioni di Capolago. J. zeichnete sich v.a. als Naturwissenschaftler, Forscher und Vermittler aus, der die Botanik einem breiteren Publikum zug\u00e4nglich machte. Er verfasste Beitr\u00e4ge f\u00fcr den \"Bollettino della Societ\u00e0 ticinese di scienze naturali\" (1905-54) und Publikationen \u00fcber die Flora des subalpinen Raumes. Ferner war er f\u00fcr die Abteilung der Tessiner Naturwissenschaftler in der Anthologie \"Scrittori della Svizzera italiana\" (1936) verantwortlich. 1937 Ehrenb\u00fcrger von Bodio."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q21516888", "itemLabel": "Q21516888", "dhsid": "023267", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "132874105"}], "wikidata_url": "http://www.wikidata.org/entity/Q21516888", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "J", "url": "https://hls-dhs-dss.ch/de/articles/023267/2007-05-01"}