ur": [{"text": "B. Wyder, Edmond B., Ausstellungskat. Martigny, 1979", "author": ["B. Wyder"], "tpub": ["Edmond B., Ausstellungskat. Martigny, 1979"]}, {"text": "P. Ruedin, Edmond B., Liz. Lausanne, 1991", "author": ["P. Ruedin"], "tpub": ["Edmond B., Liz. Lausanne, 1991"]}, {"text": "M. Hiroz-Farquet, Edmond B., Liz. Freiburg, 1997", "author": ["M. Hiroz-Farquet"], "tpub": ["Edmond B., Liz. Freiburg, 1997"]}, {"text": "BLSK, 111 f.", "tpub": ["BLSK, 111 f."]}, {"text": "C.-A. K\u00fcnzi, Edmond B., Liz. Neuenburg, 2001", "author": ["C.-A. K\u00fcnzi"], "tpub": ["Edmond B., Liz. Neuenburg, 2001"]}]}, "metagrid_id": "27503", "metagrid_links": [{"BSG": {"url": "https://www.bsg.nb.admin.ch/discovery/search?query=lds50,contains,120118386&vid=41SNL_54_INST:bsg", "short_description": "BSG", "long_description": "Bibliographie der Schweizergeschichte"}, "Sikart": {"url": "http://www.sikart.ch/kuenstlerinnen.aspx?id=4000032", "short_description": "SIKART", "long_description": "SIKART"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/120118386"}, {"title": "VIAF", "url": "http://viaf.org/viaf/39501503"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 24.1.1878\n   \u271d\ufe0e 8.3.1959\n1878-01-241959-03-08", "birth_date": "1878-01-24", "death_date": "1959-03-08"}], "birth_date": "1878-01-24", "death_date": "1959-03-08", "text_blocks": [["h1", "Edmond Bille"], ["p", "24.1.1878 Valangin, 8.3.1959 Siders, ref., von Boudevilliers. Sohn des Ernest, Schuldirektors. \u200b1) Elisa Mayor, Tochter des Ernest, Kaufmanns, 2) Catherine Tapperel, Tochter des Pierre-Joseph, Landwirts. Stud. an der Ecole des Beaux-Arts in Genf, der Ecole nationale des Arts d\u00e9coratifs und an der Acad\u00e9mie Julian in Paris. Ab 1904 in Siders wohnhaft. Genre- und Landschaftsmaler, Druck- und Werbegraphiker, Buchillustrator (Charles Ferdinand Ramuz, Gonzague de Reynold), Glas- und Wandmaler (Kirchenfenster und Wandmalereien in Siders, Martigny, Fully, Chamoson, Saint-Maurice, Lausanne, Saint-Blaise). Als Walliser Politiker (zun\u00e4chst Radikaler, sp\u00e4ter Sozialist), Schriftsteller, Journalist und Kunstkritiker (z.T. unter dem Pseudonym Pierre Bertol) nahm B. aktiv teil an den Auseinandersetzungen seiner Zeit: Pazifismus (Gr\u00fcnder der Zeitschrift \"L'Arbal\u00e8te\"), Schweizer Kunst, Modernit\u00e4t, angewandte und religi\u00f6se Kunst. 1923-36 Vorstandsmitglied des \u0152uvre, 1925-36 Mitglied der Eidg. Komm. f\u00fcr angewandte Kunst."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q681741", "itemLabel": "Edmond Bille", "dhsid": "027503", "namefr": "Edmond Bille", "articlefr": "https://fr.wikipedia.org/wiki/Edmond_Bille", "namede": "Edmond Bille", "articlede": "https://de.wikipedia.org/wiki/Edmond_Bille", "nameit": "", "articleit": "", "nameen": "Edmond Bille", "articleen": "https://en.wikipedia.org/wiki/Edmond_Bille", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "120118386"}], "wikidata_url": "http://www.wikidata.org/entity/Q681741", "wikipedia_page_title": "Edmond Bille", "added_wikidata_wikipedia_to_text_links": true, "_initial": "B", "url": "https://hls-dhs-dss.ch/de/articles/027503/2002-11-11"}