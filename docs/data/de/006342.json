i_links": [{"item": "http://www.wikidata.org/entity/Q1292447", "itemLabel": "Fritz Gr\u00fctter", "dhsid": "006342", "namefr": "", "articlefr": "", "namede": "Fritz Gr\u00fctter", "articlede": "https://de.wikipedia.org/wiki/Fritz_Gr%C3%BCtter", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1062513614"}], "wikidata_url": "http://www.wikidata.org/entity/Q1292447", "wikipedia_page_title": "Fritz Gr\u00fctter", "added_wikidata_wikipedia_to_text_links": true, "_initial": "G", "url": "https://hls-dhs-dss.ch/de/articles/006342/2006-03-16"}