hrenlegion."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q643303", "itemLabel": "Jules Girardet", "dhsid": "022410", "namefr": "Jules Girardet", "articlefr": "https://fr.wikipedia.org/wiki/Jules_Girardet", "namede": "Jules Girardet", "articlede": "https://de.wikipedia.org/wiki/Jules_Girardet", "nameit": "Jules Girardet", "articleit": "https://it.wikipedia.org/wiki/Jules_Girardet", "nameen": "Jules Girardet", "articleen": "https://en.wikipedia.org/wiki/Jules_Girardet", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "14365182X"}], "wikidata_url": "http://www.wikidata.org/entity/Q643303", "wikipedia_page_title": "Jules Girardet", "added_wikidata_wikipedia_to_text_links": true, "_initial": "G", "url": "https://hls-dhs-dss.ch/de/articles/022410/2007-07-11"}