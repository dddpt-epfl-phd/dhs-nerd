otice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/1053923392"}, {"title": "VIAF", "url": "http://viaf.org/viaf/309642674"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 28.9.1757\n   \u271d\ufe0e 3.2.1841\n1757-09-281841-02-03", "birth_date": "1757-09-28", "death_date": "1841-02-03"}], "birth_date": "1757-09-28", "death_date": "1841-02-03", "text_blocks": [["h1", "Konrad Schindler"], ["p", "28.9.1757 Mollis (heute Gem. Glarus Nord), 3.2.1841 Mollis, ref., von Mollis. Sohn des Fridolin, Zeugherrn, und der Anna Zwicky. Bruder des Kaspar (->). 1776 Dorothea Zwicky, Tochter des Johann Heinrich, Pfarrers. Besuch des Philanthropinums in Haldenstein und der Pfeffel'schen Kriegsakad. in Colmar, Architekturstudien in Paris. Erbauer der Herrensitze Haltli und Hof in Mollis. 1791 Ratsherr, 1798-99 Pr\u00e4s. der Verwaltungskammer des Kt. Linth. 1800 Wahl in den kant. Erziehungsrat. 1802 Mitglied der Notabelnversammlung und des Verfassungsrats f\u00fcr den Kt. Glarus. 1803 Mitwirkung bei der Einf\u00fchrung der Mediationsverfassung in Glarus. 1803-04 Neunerrichter, 1820-30 Strassendirektor (Bau der ersten, f\u00fcr die damalige Zeit modernen Strasse des Kantons von N\u00e4fels nach Oberurnen). Neben Hans Conrad Escher eifrigster F\u00f6rderer des Linthwerks; nach dessen Tod 1823 \u00dcbernahme der Oberleitung des Projekts. 1816 Mitgr\u00fcnder der Evang. Hilfsgesellschaft. S., der den Ideen der franz. Revolution offen gegen\u00fcberstand, machte sich um die Besiedlung der Linthebene, die Armenschule Linthkolonie und die Intensivierung des Ackerbaus verdient."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q98556450", "itemLabel": "Q98556450", "dhsid": "017931", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": ""}], "wikidata_url": "http://www.wikidata.org/entity/Q98556450", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "S", "url": "https://hls-dhs-dss.ch/de/articles/017931/2011-08-08"}