 1962-1963."]], "text_links": [[{"start": 0, "end": 11, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "PERSON", "extra_fields": {"rawName": "Hannes", "confidence_score": 0, "domains": ["Enterprise"], "origin": "entity_fishing"}}}], [{"start": 78, "end": 80, "mention": "->", "href": "/fr/articles/005588/2021-04-27/", "dhsid": "005588", "wiki_links": [], "wikidata_url": null, "wikipedia_page_title": null}, {"start": 0, "end": 4, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q12387051", "wikipedia_page_id": 6954198, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q12387051", "grobid_tag": null, "extra_fields": {"rawName": "Hannes", "confidence_score": 0, "domains": ["Enterprise"], "origin": "entity_fishing"}}}, {"start": 12, "end": 21, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "BUSINESS", "extra_fields": {"rawName": "Hannes", "confidence_score": 0, "domains": ["Enterprise"], "origin": "entity_fishing"}}}, {"start": 35, "end": 44, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "BUSINESS", "extra_fields": {"rawName": "Hannes", "confidence_score": 0, "domains": ["Enterprise"], "origin": "entity_fishing"}}}, {"start": 55, "end": 62, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q69407", "wikipedia_page_id": 218288, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q69407", "grobid_tag": null, "extra_fields": {"rawName": "Hannes", "confidence_score": 0, "domains": ["Enterprise"], "origin": "entity_fishing"}}, 