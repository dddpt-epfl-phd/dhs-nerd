ingeschichte."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q19001567", "itemLabel": "Burkhard Reber", "dhsid": "029599", "namefr": "", "articlefr": "", "namede": "Burkhard Reber", "articlede": "https://de.wikipedia.org/wiki/Burkhard_Reber", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "118833820"}], "wikidata_url": "http://www.wikidata.org/entity/Q19001567", "wikipedia_page_title": "Burkhard Reber", "added_wikidata_wikipedia_to_text_links": true, "_initial": "R", "url": "https://hls-dhs-dss.ch/de/articles/029599/2016-11-25"}