ull, "extra_fields": {"rawName": "Bern", "confidence_score": 0.6142, "domains": ["Nautical", "Transport", "Environment", "Gas", "Architecture", "Geology", "Pedagogy", "Sociology"], "origin": "entity_fishing"}}}, {"start": 1048, "end": 1052, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q70", "wikipedia_page_id": 3723873, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q70", "grobid_tag": null, "extra_fields": {"rawName": "Bern", "confidence_score": 0.6142, "domains": ["Nautical", "Transport", "Environment", "Gas", "Architecture", "Geology", "Pedagogy", "Sociology"], "origin": "entity_fishing"}}, "href": "de/articles/000209", "dhsid": "000209", "wiki": {"item": "http://www.wikidata.org/entity/Q70", "itemLabel": "Berne", "dhsid": "000209", "namefr": "Berne", "articlefr": "https://fr.wikipedia.org/wiki/Berne", "namede": "Bern", "articlede": "https://de.wikipedia.org/wiki/Bern", "nameit": "Berna", "articleit": "https://it.wikipedia.org/wiki/Berna", "nameen": "Bern", "articleen": "https://en.wikipedia.org/wiki/Bern", "instanceof": "http://www.wikidata.org/entity/Q537250", "instanceofLabel": "ville f\u00e9d\u00e9rale", "gndid": "4005762-8", "wikidata_id": "Q70"}}]], "sources": {"Werke": [{"text": "GW, 4 Bde., 1965-68", "tpub": ["GW, 4 Bde., 1965-68"]}], "Literatur": [{"text": "E. Blatter, Maria L. (1891-1973), [1991]", "author": ["E. Blatter"], "tpub": ["Maria L. (1891-1973), [1991]"]}]}, "metagrid_id": "12070", "metagrid_links": [{"BSG": {"url": "https://www.bsg.nb.admin.ch/discovery/search?query=lds50,contains,119429047&vid=41SNL_54_INST:bsg", "short_description": "BSG", "long_description": "Bibliographie der Schweizergeschichte"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/119429047"}, {"title": "VIAF", "url": "http://viaf.org/viaf/20490608"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 25.8.1891\n   \u271d\ufe0e 4.7.1973\n1891-08-251973-07-04", "birth_date": "1891-08-25", "death_date": "1973-07-04"}], "birth_date": "1891-08-25", "death_date": "1973-07-04", "text_blocks": [["h1", "Maria Lauber"], ["p", "25.8.1891 Prasten (Gem. Frutigen), 4.7.1973 Frutigen, ref., von Frutigen. Tochter des Johannes und der Rosina Susanna geb. Grossen. Ledig. Lehrerseminar Monbijou in Bern. 1910-24 Primarlehrerin in Adelboden, Neuligen (Gem. Eriswil) und Oberried (Gem. Lenk), 1924-52 Lehrt\u00e4tigkeit an der Unterschule in Kien bei Reichenbach, 1952 fr\u00fchzeitige Pensionierung aus gesundheitl. Gr\u00fcnden. In ihrem fr\u00fchen schriftsteller. Schaffen in Hochdeutsch und Frutig-Dialekt bilden volkskundlich ausgerichtete Arbeiten \u00fcber das Frutigtal (\"Alpen-Legendchen\" 1920, \"Wa Grossatt nug het gl\u00e4bt\" 1939, \"Hab Sorg derzue\" 1946) den Schwerpunkt. Mit den autobiograf. Erz\u00e4hlungen \"Ch\u00fcngold\" (1950), die sie auf Anraten von Otto von Greyerz aus einem hochdt. Manuskript in den Frutig-Dialekt umarbeitete, und \"Ch\u00fcngold in der Stadt\" (1954) sowie mit ihren Gedichten (\"Mis Tal\" 1955, \"Bletter im Luft\" 1959) schuf L. von Verhaltenheit und innerer Erregung getragene Mundartwerke von grosser Intensit\u00e4t. 1951 Buchpreis der Schweiz. Schillerstiftung, 1966 Literaturpreis des Kt. Bern."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q52044", "itemLabel": "Maria Lauber", "dhsid": "012070", "namefr": "", "articlefr": "", "namede": "Maria Lauber", "articlede": "https://de.wikipedia.org/wiki/Maria_Lauber", "nameit": "", "articleit": "", "nameen": "Maria Lauber", "articleen": "https://en.wikipedia.org/wiki/Maria_Lauber", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "119429047"}], "wikidata_url": "http://www.wikidata.org/entity/Q52044", "wikipedia_page_title": "Maria Lauber", "added_wikidata_wikipedia_to_text_links": true, "_initial": "L", "url": "https://hls-dhs-dss.ch/de/articles/012070/2008-11-25"}