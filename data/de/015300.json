inks": [{"item": "http://www.wikidata.org/entity/Q69342", "itemLabel": "Johann Amerbach", "dhsid": "015300", "namefr": "Johann Amerbach", "articlefr": "https://fr.wikipedia.org/wiki/Johann_Amerbach", "namede": "Johann Amerbach", "articlede": "https://de.wikipedia.org/wiki/Johann_Amerbach", "nameit": "", "articleit": "", "nameen": "Johann Amerbach", "articleen": "https://en.wikipedia.org/wiki/Johann_Amerbach", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "123073189"}], "wikidata_url": "http://www.wikidata.org/entity/Q69342", "wikipedia_page_title": "Johann Amerbach", "added_wikidata_wikipedia_to_text_links": true, "_initial": "A", "url": "https://hls-dhs-dss.ch/de/articles/015300/2010-10-07"}