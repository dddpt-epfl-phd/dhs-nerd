nks": [{"item": "http://www.wikidata.org/entity/Q96410982", "itemLabel": "Boris Tschlenoff", "dhsid": "049696", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1064720307"}], "wikidata_url": "http://www.wikidata.org/entity/Q96410982", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "T", "url": "https://hls-dhs-dss.ch/de/articles/049696/2011-09-30"}