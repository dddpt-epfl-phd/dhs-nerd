g": null, "extra_fields": {"rawName": "17. Jh", "confidence_score": 0.4052, "domains": ["Biology", "Sociology", "Physics", "Geology", "Oceanography", "Earth", "Chemistry"], "origin": "entity_fishing"}}}, {"start": 875, "end": 881, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q7016", "wikipedia_page_id": 5848, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q7016", "grobid_tag": null, "extra_fields": {"rawName": "17. Jh", "confidence_score": 0.4052, "domains": ["Biology", "Sociology", "Physics", "Geology", "Oceanography", "Earth", "Chemistry"], "origin": "entity_fishing"}}}]], "sources": {"Literatur": [{"text": "C. von Steiger, Innere Probleme des bern. Patriziats, 1954", "author": ["C. von Steiger"], "tpub": ["Innere Probleme des bern. Patriziats, 1954"]}, {"text": "Feller, Bern 2", "author": ["Feller"], "tpub": ["Bern 2"]}]}, "metagrid_id": "16664", "metagrid_links": [{}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/123733170"}, {"title": "VIAF", "url": "http://viaf.org/viaf/35371597"}], "bref": [{"title": "Famili\u00e4re Zugeh\u00f6rigkeit", "text": "Dachselhofer", "link": ["/articles/020866/2010-03-17/"]}, {"title": "Lebensdaten", "text": "\u2248\ufe0e 18.11.1595\n   \u271d\ufe0e 12.2.1670\n1670-02-12", "death_date": "1670-02-12"}], "death_date": "1670-02-12", "text_blocks": [["h1", "Niklaus Dachselhofer"], ["p", "18.11.1595 Bern, 12.2.1670 Bern, ref., von Bern. Sohn des Niklaus, Landvogts zu Thun, und der Anna G\u00fcder. \u200b1) 1618 Barbara St\u00f6lli (1641), 2) 1643 Magdalena Steiger (1645), 3) 1647 Barbara von B\u00fcren. 1628 Mitglied des Berner Gr. Rats und Heimlicher, 1629 Kleinrat, 1630-34 Landvogt zu Yverdon, 1635 Kleinrat und Deutschseckelmeister, 1636-67 abwechselnd amtierender und stillstehender Schultheiss. D. vertrat das bern. Staatsinteresse in schwierigsten Phasen des patriz. Regiments: Er leitete die Kommissionen zur Untersuchung der Veruntreuungen von Seckelmeister Johann Frischherz 1639 und zur Aufkl\u00e4rung der Hintergr\u00fcnde des Thuner Aufstands 1641. Im Bauernkrieg vertrat er an der Landsgemeinde in Langnau im Emmental (3./13.3.1653) erfolglos den obrigkeitl. Standpunkt. D. war ein bedeutender Exponent des bern. Patriziats z.Z. der Abschliessung der Burgerschaft Mitte des 17. Jh."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q7035384", "itemLabel": "Niclaus Dachselhoffer", "dhsid": "016664", "namefr": "", "articlefr": "", "namede": "Niklaus Dachselhofer (Schultheiss)", "articlede": "https://de.wikipedia.org/wiki/Niklaus_Dachselhofer_(Schultheiss)", "nameit": "", "articleit": "", "nameen": "Niklaus Dachselhofer", "articleen": "https://en.wikipedia.org/wiki/Niklaus_Dachselhofer", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "123733170"}], "wikidata_url": "http://www.wikidata.org/entity/Q7035384", "wikipedia_page_title": "Niklaus Dachselhofer (Schultheiss)", "added_wikidata_wikipedia_to_text_links": true, "_initial": "D", "url": "https://hls-dhs-dss.ch/de/articles/016664/2004-03-16"}