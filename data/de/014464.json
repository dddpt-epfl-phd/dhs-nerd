notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/124247326"}, {"title": "VIAF", "url": "http://viaf.org/viaf/15699866"}], "bref": [{"title": "Variante(n)", "text": "Franz Ludwig Pfyffer"}, {"title": "Famili\u00e4re Zugeh\u00f6rigkeit", "text": "Pfyffer", "link": ["/articles/022243/2017-03-15/"]}, {"title": "Lebensdaten", "text": "\u2217\ufe0e 18.5.1716\n   \u271d\ufe0e 7.11.1802\n1716-05-181802-11-07", "birth_date": "1716-05-18", "death_date": "1802-11-07"}], "birth_date": "1716-05-18", "death_date": "1802-11-07", "text_blocks": [["h1", "Franz Ludwig Pfyffer von Wyher"], ["p", "18.5.1716 Luzern, 7.11.1802 Luzern, kath., von Luzern. Sohn des Jost Franz, Kleinrats und Hauptmanns in franz. Diensten. \u200bMarie Josse d'Hemel, aus Argenteuil bei Paris. Ab 1726 Kadettenschule in Paris, 1736 Hauptmann in franz. Diensten, 1763 Oberst, 1769 Resignation als Generalleutnant. 1736 Luzerner Grossrat, 1752 Kleinrat. 1779 Mitgr\u00fcnder und 1781-86 Pr\u00e4s. der Helvet.-Milit\u00e4r. Gesellschaft. Angeregt durch den Reliefbau f\u00fcr Festungen in Frankreich schuf P. aus topograf. Interesse die ersten Gebirgsreliefs der Schweiz (1750 Pilatus; 1762-86 Relief der Urschweiz, heute im Museum des Gletschergartens Luzern) und gilt als Begr\u00fcnder des schweiz. Reliefbaus. Grundlage waren seine eigenen Zeichnungen und die f\u00fcr die damalige Zeit relativ genauen Messungen. Seine Karten zeigten erstmals die genauen Umrisse des Vierwaldst\u00e4ttersees."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q1448095", "itemLabel": "Franz Ludwig Pfyffer von Wyer", "dhsid": "014464", "namefr": "Franz Ludwig Pfyffer", "articlefr": "https://fr.wikipedia.org/wiki/Franz_Ludwig_Pfyffer", "namede": "Franz Ludwig Pfyffer", "articlede": "https://de.wikipedia.org/wiki/Franz_Ludwig_Pfyffer", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "124247326"}], "wikidata_url": "http://www.wikidata.org/entity/Q1448095", "wikipedia_page_title": "Franz Ludwig Pfyffer", "added_wikidata_wikipedia_to_text_links": true, "_initial": "P", "url": "https://hls-dhs-dss.ch/de/articles/014464/2010-09-28"}