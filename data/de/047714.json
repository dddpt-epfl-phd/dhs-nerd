dia_page_id": 490244, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q5891", "grobid_tag": null, "extra_fields": {"rawName": "Conrad-Ferdinand-Meyer-Preis", "confidence_score": 0.981, "domains": ["Sociology"], "origin": "entity_fishing"}}}, {"start": 782, "end": 810, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q681604", "wikipedia_page_id": 149428, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q681604", "grobid_tag": null, "extra_fields": {"rawName": "Conrad-Ferdinand-Meyer-Preis", "confidence_score": 0.981, "domains": ["Sociology"], "origin": "entity_fishing"}}}]], "sources": {}, "metagrid_id": "47714", "metagrid_links": [{}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/115434992"}, {"title": "VIAF", "url": "http://viaf.org/viaf/54878303"}], "bref": [{"title": "Variante(n)", "text": "Elfriede Abrahamowicz (Taufname)\nElfriede Huber (Ehename)"}, {"title": "Lebensdaten", "text": "\u2217\ufe0e 19.12.1922\n   \u271d\ufe0e 15.7.2001\n1922-12-192001-07-15", "birth_date": "1922-12-19", "death_date": "2001-07-15"}], "birth_date": "1922-12-19", "death_date": "2001-07-15", "text_blocks": [["h1", "Elfriede Huber-Abrahamowicz"], ["p", "19.12.1922 Wien, 15.7.2001 Z\u00fcrich, ref., urspr\u00fcnglich aus \u00d6sterreich, ab 1948 von Basel und Oberkulm. Tochter des Jakob Abrahamowicz, Arztes aus Pojor\u00e2ta (Rum\u00e4nien), und der Helene geb. Socher, aus Jihlava (B\u00f6hmen). 1948 Gerhard Huber (->). 1939 Emigration mit ihrer Fam. in die Schweiz. 1943-50 Stud. der Philosophie, Germanistik und Psychologie in Basel, 1950 Promotion als erste Doktorandin bei Heinrich Barth. Ab 1956 war H. als Schriftstellerin und Gastdozentin t\u00e4tig. An der ETH und am Alfred-Adler-Institut in Z\u00fcrich unterrichtete sie Philosophie des Feminismus und Kunstphilosophie. H. schrieb Prosa (\"Der unendliche Weg\" 1964, die autobiograf. \"Erstbesteigung\" 2003), Lyrik (\"Verh\u00e4ngnis\" 1957, \"Hoffnungslos hoffend\" 1989) und wissenschaftl. Aufs\u00e4tze zur Philosophie. 1965 Conrad-Ferdinand-Meyer-Preis."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q27043316", "itemLabel": "Elfriede Huber-Abrahamowicz", "dhsid": "047714", "namefr": "", "articlefr": "", "namede": "Elfriede Huber-Abrahamowicz", "articlede": "https://de.wikipedia.org/wiki/Elfriede_Huber-Abrahamowicz", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "115434992"}], "wikidata_url": "http://www.wikidata.org/entity/Q27043316", "wikipedia_page_title": "Elfriede Huber-Abrahamowicz", "added_wikidata_wikipedia_to_text_links": true, "_initial": "H", "url": "https://hls-dhs-dss.ch/de/articles/047714/2007-10-29"}