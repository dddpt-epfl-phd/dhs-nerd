iki_links": [{"item": "http://www.wikidata.org/entity/Q29012633", "itemLabel": "Max Lichtegg", "dhsid": "009516", "namefr": "", "articlefr": "", "namede": "Max Lichtegg", "articlede": "https://de.wikipedia.org/wiki/Max_Lichtegg", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "133044130"}], "wikidata_url": "http://www.wikidata.org/entity/Q29012633", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/it/articles/009516/2008-11-27"}