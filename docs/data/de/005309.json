tionen."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q22926427", "itemLabel": "Rudolf Antoni Gianzun", "dhsid": "005309", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": ""}], "wikidata_url": "http://www.wikidata.org/entity/Q22926427", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/de/articles/005309/2005-08-11"}