tice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/1051427258"}, {"title": "VIAF", "url": "http://viaf.org/viaf/308722603"}], "bref": [{"title": "Dati biografici", "text": "\u2217\ufe0e 5.10.1814\n   \u271d\ufe0e 21.1.1905\n1814-10-051905-01-21", "birth_date": "1814-10-05", "death_date": "1905-01-21"}], "birth_date": "1814-10-05", "death_date": "1905-01-21", "text_blocks": [["h1", "Johann Jakob Bucher"], ["p", "5.10.1814 Niederweningen, 21.1.1905 Regensberg, rif., di Niederweningen, nel 1863 cittadino onorario di Regensberg. Figlio di Heinrich, bottaio, falegname e agricoltore. \u200b(1847) Louisa Studer, figlia del medico com. di Winterthur. Frequent\u00f2 una scuola privata a Regensberg, dove fu assistente di cancelleria (1832-36); fino al 1836 fu inoltre segr. della cancelleria del tribunale penale di Zurigo. Dal 1832 al 1839 studi\u00f2 diritto all'Univ. di Zurigo (recuperando l'esame di maturit\u00e0 nel 1838); dopo soggiorni di studio a Losanna, Parigi e Londra, nel 1844 sostenne l'esame di procuratore e dal 1847 al 1851 lavor\u00f2 come avvocato a Zurigo. Fu segr. del tribunale distr. di Regensberg dal 1851 al 1869, anno in cui gli venne tolto l'incarico perch\u00e9 zelante sostenitore di Alfred Escher, suo compagno di studi. In seguito, e fino al 1903, esercit\u00f2 l'avvocatura a Regensberg. Liberale, fu Granconsigliere zurighese (1853-66) e Consigliere nazionale (1857-66); si dedic\u00f2 al miglioramento delle infrastrutture del suo com. di residenza (approvvigionamento idrico, telefono e telegrafo)."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q18816507", "itemLabel": "Johann Jakob Bucher", "dhsid": "003620", "namefr": "", "articlefr": "", "namede": "Johann Jakob Bucher", "articlede": "https://de.wikipedia.org/wiki/Johann_Jakob_Bucher", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1051427258"}], "wikidata_url": "http://www.wikidata.org/entity/Q18816507", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/it/articles/003620/1999-03-19"}