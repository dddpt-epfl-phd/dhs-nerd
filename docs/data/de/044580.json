nks": [{"item": "http://www.wikidata.org/entity/Q115929", "itemLabel": "Hermann Fol", "dhsid": "044580", "namefr": "Hermann Fol", "articlefr": "https://fr.wikipedia.org/wiki/Hermann_Fol", "namede": "Hermann Fol", "articlede": "https://de.wikipedia.org/wiki/Hermann_Fol", "nameit": "Hermann Fol", "articleit": "https://it.wikipedia.org/wiki/Hermann_Fol", "nameen": "Hermann Fol", "articleen": "https://en.wikipedia.org/wiki/Hermann_Fol", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1055361472"}], "wikidata_url": "http://www.wikidata.org/entity/Q115929", "wikipedia_page_title": "Hermann Fol", "added_wikidata_wikipedia_to_text_links": true, "_initial": "F", "url": "https://hls-dhs-dss.ch/de/articles/044580/2007-04-18"}