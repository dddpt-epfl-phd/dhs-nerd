rid_id": "10823", "metagrid_links": [{"Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=78764", "short_description": "Schweizerische Eliten im 20. Jahrhundert", "long_description": "Schweizerische Eliten im 20. Jahrhundert"}, "BSG": {"url": "https://www.bsg.nb.admin.ch/discovery/search?query=lds50,contains,119045397&vid=41SNL_54_INST:bsg", "short_description": "BSG", "long_description": "Bibliographie der Schweizergeschichte"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/119045397"}, {"title": "VIAF", "url": "http://viaf.org/viaf/98449299"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 20.7.1919\n   \u271d\ufe0e 4.4.2015\n1919-07-202015-04-04", "birth_date": "1919-07-20", "death_date": "2015-04-04"}], "birth_date": "1919-07-20", "death_date": "2015-04-04", "text_blocks": [["h1", "Martin Anton Schmidt"], ["p", "20.7.1919 Wernburg (Th\u00fcringen),4.4.2015 Steffisburg, ref., Deutscher, ab 1942 von Riehen. Sohn des Karl Ludwig (->). 1950 Ruth Kempter, Tochter des Josef, Kaufmanns. Stud. der Theologie an den Univ. Basel (1948 Dr. theol.) und Z\u00fcrich. 1942-43 Lehrvikariat in Payerne, 1943-45 Religionslehrer in der Evang. Lehranstalt Schiers, 1945-46 Vikar und 1946-47 Pfarrhelfer in Z\u00fcrich-Seebach. 1947-55 Pfarrer der Kirchgem. Kilchberg-R\u00fcnenberg-Zeglingen. 1951 Habilitation und PD f\u00fcr Kirchen- und Dogmengeschichte an der Univ. Basel, 1953-54 Gastprof. an der kirchl. Hochschule in Berlin, 1955-58 Lehrt\u00e4tigkeit an verschiedenen nordamerikan. Universit\u00e4ten, 1961-67 Prof. f\u00fcr Kirchengeschichte am San Francisco Theological Seminary in San Anselmo. 1967-89 Prof. f\u00fcr Kirchen- und Dogmengeschichte an der Univ. Basel. Seinen Ruf als Medi\u00e4visten begr\u00fcndete S. haupts\u00e4chlich mit seinen Arbeiten zur Scholastik."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q20872553", "itemLabel": "Martin Anton Schmidt", "dhsid": "010823", "namefr": "