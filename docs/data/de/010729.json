ins."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q87063553", "itemLabel": "Wilhelm Legrand", "dhsid": "010729", "namefr": "", "articlefr": "", "namede": "Wilhelm Legrand (Geistlicher)", "articlede": "https://de.wikipedia.org/wiki/Wilhelm_Legrand_(Geistlicher)", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "120624982"}], "wikidata_url": "http://www.wikidata.org/entity/Q87063553", "wikipedia_page_title": "Wilhelm Legrand (Geistlicher)", "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/de/articles/010729/2007-11-29"}