ticlede": "https://de.wikipedia.org/wiki/Balthasar_Ambuel", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": ""}], "wikidata_url": "http://www.wikidata.org/entity/Q71131899", "wikipedia_page_title": "Balthasar Ambuel", "added_wikidata_wikipedia_to_text_links": true, "_initial": "A", "url": "https://hls-dhs-dss.ch/de/articles/010514/2001-07-17"}