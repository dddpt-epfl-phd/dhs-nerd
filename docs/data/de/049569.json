inks": [{"item": "http://www.wikidata.org/entity/Q2163726", "itemLabel": "Rolf Rappaz", "dhsid": "049569", "namefr": "", "articlefr": "", "namede": "Rolf Rappaz", "articlede": "https://de.wikipedia.org/wiki/Rolf_Rappaz", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "119319586"}], "wikidata_url": "http://www.wikidata.org/entity/Q2163726", "wikipedia_page_title": "Rolf Rappaz", "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/de/articles/049569/2010-07-27"}