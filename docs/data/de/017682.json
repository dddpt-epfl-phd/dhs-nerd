 Pruntrut."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q2854182", "itemLabel": "Antoine L\u00e9mane", "dhsid": "017682", "namefr": "Antoine L\u00e9mane", "articlefr": "https://fr.wikipedia.org/wiki/Antoine_L%C3%A9mane", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1074354567"}], "wikidata_url": "http://www.wikidata.org/entity/Q2854182", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "L", "url": "https://hls-dhs-dss.ch/de/articles/017682/2006-06-27"}