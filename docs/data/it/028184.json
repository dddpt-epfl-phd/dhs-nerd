68."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q61078", "itemLabel": "Walter Benjamin", "dhsid": "028184", "namefr": "Walter Benjamin", "articlefr": "https://fr.wikipedia.org/wiki/Walter_Benjamin", "namede": "Walter Benjamin", "articlede": "https://de.wikipedia.org/wiki/Walter_Benjamin", "nameit": "Walter Benjamin", "articleit": "https://it.wikipedia.org/wiki/Walter_Benjamin", "nameen": "Walter Benjamin", "articleen": "https://en.wikipedia.org/wiki/Walter_Benjamin", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "118509039"}], "wikidata_url": "http://www.wikidata.org/entity/Q61078", "wikipedia_page_title": "Walter Benjamin", "added_wikidata_wikipedia_to_text_links": true, "_initial": "B", "url": "https://hls-dhs-dss.ch/it/articles/028184/2002-06-25"}