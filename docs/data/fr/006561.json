ratif."]], "text_links": [[{"start": 0, "end": 10, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "PERSON", "extra_fields": {"rawName": "mouvement coop\u00e9ratif", "confidence_score": 0.8121, "domains": ["Administration", "Animals", "Psychiatry", "Health"], "origin": "entity_fishing"}}}], [{"start": 11, "end": 20, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q581647", "wikipedia_page_id": 1005486, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q581647", "grobid_tag": null, "extra_fields": {"rawName": "mouvement coop\u00e9ratif", "confidence_score": 0.8121, "domains": ["Administration", "Animals", "Psychiatry", "Health"], "origin": "entity_fishing"}}, "href": "fr/articles/001191", "dhsid": "001191", "wiki": {"item": "http://www.wikidata.org/entity/Q581647", "itemLabel": "Arlesheim", "dhsid": "001191", "namefr": "Arlesheim", "articlefr": "https://fr.wikipedia.org/wiki/Arlesheim", "namede": "Arlesheim", "articlede": "https://de.wikipedia.org/wiki/Arlesheim", "nameit": "Arlesheim", "articleit": "https://it.wikipedia.org/wiki/Arlesheim", "nameen": "Arlesheim", "articleen": "https://en.wikipedia.org/wiki/Arlesheim", "instanceof": "http://www.wikidata.org/entity/Q54935504", "instanceofLabel": "ville de Suisse", "gndid": "4002929-3", "wikidata_id": "Q581647"}}, {"start": 34, "end": 43, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q66736", "wikipedia_page_id": 1068918, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q66736", "grobid_tag": null, "extra_fields": {"rawName": "mouvement coop\u00e9ratif", "confidence_score": 0.8121, "domains": ["Administration", "Animals", "Psychiatry", "Health"], "origin": "entity_fishing"}}, "href": "fr/articles/000382", "dhsid": "000382", "wiki": {"item": 