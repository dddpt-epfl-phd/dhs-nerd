tagrid_links": [{"Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=56293", "short_description": "Schweizerische Eliten im 20. Jahrhundert", "long_description": "Schweizerische Eliten im 20. Jahrhundert"}, "Sikart": {"url": "http://www.sikart.ch/kuenstlerinnen.aspx?id=4023397", "short_description": "SIKART", "long_description": "SIKART"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/11870740X"}, {"title": "VIAF", "url": "http://viaf.org/viaf/96563976"}], "bref": [{"title": "Lebensdaten", "text": "\u2217\ufe0e 1.8.1885\n   \u271d\ufe0e 18.11.1976\n1885-08-011976-11-18", "birth_date": "1885-08-01", "death_date": "1976-11-18"}], "birth_date": "1885-08-01", "death_date": "1976-11-18", "text_blocks": [["h1", "Hermann Hubacher"], ["p", "1.8.1885 Biel, 18.11.1976 Z\u00fcrich, von Biel. Sohn des Fritz, Inhabers eines Graveurateliers. Enkel des Landschaftszeichners Jakob H\u00e4uselmann. 1912 Anna Tscherter. 1900-05 Graveur-, Medaillen- und Goldschmiedlehre am Technikum in Biel. 1905 Kurs an der Ecole des Beaux-Arts in Genf, 1906-07 an der Akad. Wien. Italienreise. Entscheid f\u00fcr Bildhauerei. 1908-09 im Atelier von James Vibert in Genf, dort Bekanntschaft mit Ferdinand Hodler. Kurzaufenthalt in M\u00fcnchen, Begegnung mit Adolf von Hildebrand. 1910-16 eigenes Atelier in Bern, mehrfach in Paris. Ab 1916 in Z\u00fcrich, Beginn der Freundschaft mit dem M\u00e4zen Oskar Reinhart. Reisen nach Italien und Frankreich, 1929-30 nach \u00c4gypten und in den Sudan. 1926-29 Mitglied der Eidg. Kunstkommission. Neben Portr\u00e4tb\u00fcsten und K\u00f6pfen schuf H. Denkmal-, Brunnen- und Grabfiguren sowie bauplast. Arbeiten f\u00fcr den \u00f6ffentl. Raum, so an der Schweiz. Landesausstellung 1939. Er stellte u.a. in Paris und London aus. 1938 Internat. Preis f\u00fcr Skulptur der Biennale in Venedig, 1944 Kunstpreis der Stadt Z\u00fcrich, 1945 Ehrendoktor der Univ. Z\u00fcrich, 1955 Kunstpreis der Stadt Biel, 1960 Premio del Fiorino in Florenz."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q870322", "itemLabel": "Hermann Hubacher", "dhsid": "022450", "namefr": "", "articlefr": "", "namede": "Hermann Hubacher", "articlede": "https://de.wikipedia.org/wiki/Hermann_Hubacher", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "11870740X"}], "wikidata_url": "http://www.wikidata.org/entity/Q870322", "wikipedia_page_title": "Hermann Hubacher", "added_wikidata_wikipedia_to_text_links": true, "_initial": "H", "url": "https://hls-dhs-dss.ch/de/articles/022450/2006-08-18"}