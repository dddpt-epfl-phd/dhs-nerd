_links": [{}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/1045841854"}, {"title": "VIAF", "url": "http://viaf.org/viaf/305923979"}], "bref": [{"title": "Appartenenza familiare", "text": "Dachselhofer", "link": ["/articles/020866/2010-03-17/"]}, {"title": "Dati biografici", "text": "\u2217\ufe0e 6.11.1724\n   \u271d\ufe0e 3.10.1780\n1724-11-061780-10-03", "birth_date": "1724-11-06", "death_date": "1780-10-03"}], "birth_date": "1724-11-06", "death_date": "1780-10-03", "text_blocks": [["h1", "Johann Rudolf Dachselhofer"], ["p", "6.11.1724 Berna, 3.10.1780 Berna, rif., di Berna. Figlio di Niklaus, scoltetto di Thun, e di Margaritha Steiger. \u200b(1747) Susanna Margareth Effinger. Fu membro del Gran Consiglio di Berna (1755), Grossweibel (1758-62), Consigliere segreto (1767), membro del Piccolo Consiglio (1768-80) e tesoriere dei baliaggi romandi (1771-77). Esponente del patriziato cittadino conservatore, nel 1764 fece parte delle truppe bernesi pronte all'intervento durante il processo per alto tradimento intentato contro il conservatore Franz Plazid Schumacher dai membri illuminati del patriziato cittadino lucernese, raggruppati intorno a Valentin Meyer. Quale membro della commissione arbitrale fed., durante i disordini di Ginevra (1766) D. assunse parimenti una posizione conservatrice e filofranc. Le trattative che condusse con l'ambasciatore franc. a Soletta nel 1769 per impedire la costruzione di un porto a Versoix, in concorrenza con quello di Ginevra, restarono infruttuose fino all'intervento diplomatico della Prussia."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q55681572", "itemLabel": "Johann Rudolf Dachselhofer", "dhsid": "016662", "namefr": "", "articlefr": "", "namede": "", "articlede": "", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "1045841854"}], "wikidata_url": "http://www.wikidata.org/entity/Q55681572", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": "D", "url": "https://hls-dhs-dss.ch/it/articles/016662/2004-03-16"}