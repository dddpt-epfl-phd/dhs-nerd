ide Hiob International."]], "text_links": [[{"start": 0, "end": 15, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "PERSON", "extra_fields": {"rawName": "Hiob International", "confidence_score": 0, "domains": ["Geology", "Oceanography", "Earth"], "origin": "entity_fishing"}}}], [{"start": 12, "end": 21, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": null, "wikipedia_page_id": null, "wikipedia_page_title": null, "wikidata_entity_url": null, "grobid_tag": "LOCATION", "extra_fields": {"rawName": "Hiob International", "confidence_score": 0, "domains": ["Geology", "Oceanography", "Earth"], "origin": "entity_fishing"}}}, {"start": 38, "end": 45, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q70720", "wikipedia_page_id": 382907, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q70720", "grobid_tag": null, "extra_fields": {"rawName": "Hiob International", "confidence_score": 0, "domains": ["Geology", "Oceanography", "Earth"], "origin": "entity_fishing"}}, "href": "fr/articles/001378", "dhsid": "001378", "wiki": {"item": "http://www.wikidata.org/entity/Q70720", "itemLabel": "Nesslau", "dhsid": "001378", "namefr": "Nesslau", "articlefr": "https://fr.wikipedia.org/wiki/Nesslau", "namede": "Nesslau", "articlede": "https://de.wikipedia.org/wiki/Nesslau", "nameit": "Nesslau", "articleit": "https://it.wikipedia.org/wiki/Nesslau", "nameen": "Nesslau", "articleen": "https://en.wikipedia.org/wiki/Nesslau", "instanceof": "http://www.wikidata.org/entity/Q70208", "instanceofLabel": "commune de Suisse", "gndid": "", "wikidata_id": "Q70720"}}, {"start": 67, "end": 74, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q533721", "wikipedia_page_id": 10073125, "wikipedia_page_title": null, "wikidata_entity_url": "h