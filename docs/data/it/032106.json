ce_links": [{"title": "GND", "url": "http://d-nb.info/gnd/117128228"}, {"title": "VIAF", "url": "http://viaf.org/viaf/72163429"}], "bref": [{"title": "Dati biografici", "text": "\u2217\ufe0e 11.12.1870\n   \u271d\ufe0e 1.5.1919\n1870-12-111919-05-01", "birth_date": "1870-12-11", "death_date": "1919-05-01"}], "birth_date": "1870-12-11", "death_date": "1919-05-01", "text_blocks": [["h1", "Hermann Walser"], ["p", "11.12.1870 Bienne, 1.5.1919 Berna, rif., di Teufen (AR). Figlio di Adolf, rilegatore e commerciante, e di Elisa Marti. Fratello di Karl (->) e Robert (->). Celibe. Studi\u00f2 geografia, geologia e storia alle Univ. di Berna, Monaco di Baviera e Lipsia, conseguendo il diploma per l'insegnamento superiore (1892) e il dottorato con Eduard Br\u00fcckner (1896). Insegnante alla scuola secondaria di Herzogenbuchsee (1893-94) e al liceo di Berna (1894-1909), fu libero docente e professore straordinario (1909) e poi ordinario (1911) di geografia antropica all'Univ. di Berna. Si occup\u00f2 di mutamenti del paesaggio culturale, di studio degli insediamenti e di rapporti tra politica e geografia. Collabor\u00f2 inoltre al Dictionnaire g\u00e9ographique de la Suisse."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q78074579", "itemLabel": "Hermann Walser", "dhsid": "032106", "namefr": "", "articlefr": "", "namede": "Hermann Walser", "articlede": "https://de.wikipedia.org/wiki/Hermann_Walser", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "117128228"}], "wikidata_url": "http://www.wikidata.org/entity/Q78074579", "wikipedia_page_title": null, "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/it/articles/032106/2014-12-27"}