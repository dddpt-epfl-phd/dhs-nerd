: null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q68978", "wikipedia_page_id": 39440, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q68978", "grobid_tag": null, "extra_fields": {"rawName": "Brenzikofen", "confidence_score": 0.7205, "domains": ["Administration"], "origin": "entity_fishing"}}, "href": "de/articles/000541", "dhsid": "000541", "wiki": {"item": "http://www.wikidata.org/entity/Q68978", "itemLabel": "Thoune", "dhsid": "000541", "namefr": "Thoune", "articlefr": "https://fr.wikipedia.org/wiki/Thoune", "namede": "Thun", "articlede": "https://de.wikipedia.org/wiki/Thun", "nameit": "Thun", "articleit": "https://it.wikipedia.org/wiki/Thun", "nameen": "Thun", "articleen": "https://en.wikipedia.org/wiki/Thun", "instanceof": "http://www.wikidata.org/entity/Q54935504", "instanceofLabel": "ville de Suisse", "gndid": "4059991-7", "wikidata_id": "Q68978"}}, {"start": 1130, "end": 1132, "mention": null, "origin": "entity_fishing", "annotation": {"wikidata_entity_id": "Q66146", "wikipedia_page_id": 1084284, "wikipedia_page_title": null, "wikidata_entity_url": "http://www.wikidata.org/entity/Q66146", "grobid_tag": null, "extra_fields": {"rawName": "Brenzikofen", "confidence_score": 0.7205, "domains": ["Administration"], "origin": "entity_fishing"}}, "href": "de/articles/000354", "dhsid": "000354", "wiki": {"item": "http://www.wikidata.org/entity/Q66146", "itemLabel": "Brenzikofen", "dhsid": "000354", "namefr": "Brenzikofen", "articlefr": "https://fr.wikipedia.org/wiki/Brenzikofen", "namede": "Brenzikofen", "articlede": "https://de.wikipedia.org/wiki/Brenzikofen", "nameit": "Brenzikofen", "articleit": "https://it.wikipedia.org/wiki/Brenzikofen", "nameen": "Brenzikofen", "articleen": "https://en.wikipedia.org/wiki/Brenzikofen", "instanceof": "http://www.wikidata.org/entity/Q70208", "instanceofLabel": "commune de Suisse", "gndid": "4749374-4", "wikidata_id": "Q66146"}}]], "sources": {"Quellen": [{"text": "SSRQ BE II/11", "tpub": ["SSRQ BE II/11"]}], "Literatur": [{"text": "Dorfchronik 1236-1986, 1986", "tpub": ["Dorfchronik 1236-1986, 1986"]}]}, "metagrid_id": null, "metagrid_links": [], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/4749374-4"}], "bref": [], "text_blocks": [["h1", "Brenzikofen"], ["p", "Polit. Gem. BE, Amtsbez. Konolfingen. Die Kleingem. am Fuss der Falkenfluh (B\u00fcrglen), mit Anteil am Rothachengraben, umfasst das Dorf B. (1236 Brenzichovin), die Dorfteile Boden und B\u00e4lliz sowie Einzelh\u00f6fe. 1764 144 Einw.; 1850 347; 1900 392; 1950 342; 1980 328; 1990 488; 2000 485. Auf B\u00fcrglen ma. Erdwerk oder Vorwerk (sichtbar: Wall, Graben) der Burg Diessenberg. Im Sp\u00e4tMA hatten u.a. das Kloster Interlaken sowie Burger von Bern und Thun Grundbesitz, z.T. mit Gerichtsrechten. B. geh\u00f6rte zum Freigericht Steffisburg (1642-52 Niedergericht Wichtrach) und hochgerichtlich zum Landgericht Konolfingen, 1652-1798 zum Amt Thun, kirchlich seit jeher zu Oberdiessbach. Die Gem. teilte 1686 ihren Wald auf die H\u00f6fe auf; Arme erhielten Holz zugeteilt. Die alte Schulgemeinschaft mit Herbligen wurde 1834 nach dem Bau des Schulhauses B. aufgegeben, 1973 wieder erneuert. 1898 erhielt B. Anschluss an die Bahnlinie Thun-Burgdorf. Seit 1983 ist das zentrale Armeeverpflegungsmagazin ein neuer Arbeitgeber in der von einem b\u00e4uerl. Siedlungsbild gepr\u00e4gten Gem., die nach 1980 in den Sog der Region Thun geriet (1990 69% Wegpendler, 61% in B. Erwerbst\u00e4tige im 3. Sektor)."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q66146", "itemLabel": "Brenzikofen", "dhsid": "000354", "namefr": "Brenzikofen", "articlefr": "https://fr.wikipedia.org/wiki/Brenzikofen", "namede": "Brenzikofen", "articlede": "https://de.wikipedia.org/wiki/Brenzikofen", "nameit": "Brenzikofen", "articleit": "https://it.wikipedia.org/wiki/Brenzikofen", "nameen": "Brenzikofen", "articleen": "https://en.wikipedia.org/wiki/Brenzikofen", "instanceof": "http://www.wikidata.org/entity/Q70208", "instanceofLabel": "commune de Suisse", "gndid": "4749374-4"}], "wikidata_url": "http://www.wikidata.org/entity/Q66146", "wikipedia_page_title": "Brenzikofen", "added_wikidata_wikipedia_to_text_links": true, "_initial": "B", "url": "https://hls-dhs-dss.ch/de/articles/000354/2002-12-18"}