": [{"text": "ETH-BIB, Nachlass"}], "Literatur": [{"text": "C. Courtiau et al., Z., 2006 (mit Bibl.)", "author": ["C. Courtiau et al."], "tpub": ["Z., 2006 (mit Bibl.)"]}]}, "metagrid_id": "19957", "metagrid_links": [{"Elites suisses": {"url": "https://www2.unil.ch/elitessuisses/index.php?page=detailPerso&idIdentite=55934", "short_description": "Schweizerische Eliten im 20. Jahrhundert", "long_description": "Schweizerische Eliten im 20. Jahrhundert"}, "parl.ch": {"url": "https://www.parlament.ch/de/biografie?CouncillorId=3706", "short_description": "Members of the Swiss parliament", "long_description": "Members of the Swiss National Council and the Council of States since 1848"}}], "notice_links": [{"title": "GND", "url": "http://d-nb.info/gnd/119048280"}, {"title": "VIAF", "url": "http://viaf.org/viaf/72195686"}], "bref": [{"title": "Variante(n)", "text": "Konradin Zschokke"}, {"title": "Lebensdaten", "text": "\u2217\ufe0e 14.4.1842\n   \u271d\ufe0e 17.12.1918\n1842-04-141918-12-17", "birth_date": "1842-04-14", "death_date": "1918-12-17"}], "birth_date": "1842-04-14", "death_date": "1918-12-17", "text_blocks": [["h1", "Conrad Zschokke"], ["p", "14.4.1842 Aarau, 17.12.1918 Aarau, ref., von Malans. Sohn des Alexander, Zeichners und Kupferstechers, und der Marie Joseph geb. Voitel. Cousin von Erwin (->), Friedrich (->) und Richard (->), Neffe von Alfred (->), Emil (->), Peter Olivier (->) und Theodor (->). \u200b1) 1867 Eug\u00e9nie Faure, 2) 1909 Jeanne Antoinette Disqu\u00e9. 1859-62 Stud. des Bauingenieurwesens am Eidg. Polytechnikum in Z\u00fcrich. 1862 Eintritt in die von seinem Onkel Peter Olivier geleitete Filiale der Firma Locher & Co. in Aarau, ab 1864 Angestellter, ab 1872 Gesch\u00e4ftspartner der Ingenieure Antoine Castor und Hildevert Hersent in Paris, danach Gr\u00fcndung mehrerer Unternehmen in Frankreich und Italien. Ab 1891 Lehrbeauftragter, 1893-97 o. Prof. am Eidg. Polytechnikum, bis 1900 ehrenamtlich. 1892-1906 freisinniger aarg. Grossrat (1896-97 Pr\u00e4s.), 1897-1917 Nationalrat (1902-03 Pr\u00e4s.). 1900 gr\u00fcndete Z. die Metallbauwerke Conrad Zschokke in D\u00f6ttingen, dann - nach seiner definitiven R\u00fcckkehr nach Aarau 1909 - die Conrad Zschokke AG, die er bis zu seinem Tod leitete. Auf ihn gehen bedeutende Wasserkraftwerke zur\u00fcck, bei denen er die Technik der Druckluftgr\u00fcndung anwandte. Zudem baute er in ganz Europa und Nordafrika Br\u00fccken, Strassen und Tunnel sowie Metallkonstruktionen. Z. verfasste auch zahlreiche theoret. Werke \u00fcber die Baukunst. 1901 Dr. h.c. der naturwissenschaftl. Fakult\u00e4t der Univ. Z\u00fcrich und 1915 der ETH."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q1127106", "itemLabel": "Conradin Zschokke", "dhsid": "019957", "namefr": "", "articlefr": "", "namede": "Conradin Zschokke", "articlede": "https://de.wikipedia.org/wiki/Conradin_Zschokke", "nameit": "", "articleit": "", "nameen": "", "articleen": "", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "119048280"}], "wikidata_url": "http://www.wikidata.org/entity/Q1127106", "wikipedia_page_title": "Conradin Zschokke", "added_wikidata_wikipedia_to_text_links": true, "_initial": "Z", "url": "https://hls-dhs-dss.ch/de/articles/019957/2015-01-25"}