zeichnungen."]], "wiki_links": [{"item": "http://www.wikidata.org/entity/Q708598", "itemLabel": "Arthur Baumgarten", "dhsid": "027959", "namefr": "", "articlefr": "", "namede": "Arthur Baumgarten", "articlede": "https://de.wikipedia.org/wiki/Arthur_Baumgarten", "nameit": "", "articleit": "", "nameen": "Arthur Baumgarten", "articleen": "https://en.wikipedia.org/wiki/Arthur_Baumgarten", "instanceof": "http://www.wikidata.org/entity/Q5", "instanceofLabel": "\u00eatre humain", "gndid": "118653911"}], "wikidata_url": "http://www.wikidata.org/entity/Q708598", "wikipedia_page_title": "Arthur Baumgarten", "added_wikidata_wikipedia_to_text_links": true, "_initial": null, "url": "https://hls-dhs-dss.ch/de/articles/027959/2002-06-14"}